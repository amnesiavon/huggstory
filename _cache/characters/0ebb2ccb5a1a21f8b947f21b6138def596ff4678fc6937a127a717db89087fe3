             <ul id=\\\"sidebarNav\\\"></ul>\\n            </aside>\\n            <main id=\\\"mainContentArea\\\" class=\\\"system-content-area\\\">\\n                <p class=\\\"placeholder-message\\\">ç­‰å¾…æ•°æ®è§£æä¸æ¸²æŸ“...</p>\\n            </main>\\n        </div>\\n        <footer class=\\\"system-footer\\\">\\n            <div class=\\\"energy-bar-container\\\">\\n                <div class=\\\"energy-bar-label\\\"><span>å½“å‰ä¸–ç•Œç©¿è¶Šèƒ½é‡ï¼š</span><span class=\\\"energy-value-display\\\"><span id=\\\"currentCollectedEnergy\\\">0</span> / <span id=\\\"targetTravelEnergy\\\">0</span> <span class=\\\"currency-icon\\\">ğŸ’–</span></span></div>\\n                <div class=\\\"energy-bar-bg\\\"><div class=\\\"energy-bar-fill\\\" id=\\\"energyFill\\\">0%</div></div>\\n                <p id=\\\"footerMessage\\\"></p>\\n            </div>\\n        </footer>\\n    </div>\\n    <div class=\\\"xiao-mei-corner\\\" id=\\\"xiaoMeiInteractive\\\">\\n        <div class=\\\"xiao-mei-speech-bubble\\\" id=\\\"xiaoMeiTalk\\\"></div>\\n        <img id=\\\"xiaoMeiAvatarImg\\\" src=\\\"https://via.placeholder.com/120/4a0063/ff0000?text=?\\\" alt=\\\"å°é­…\\\" class=\\\"xiao-mei-avatar-img\\\" title=\\\"å°é­…æ­£åœ¨å¾…å‘½ï½\\\">\\n    </div>\\n\\n    <script>\\n        // --- Global State ---\\n        let parsedSystemData = null;\\n        let currentActiveTab = 'Dashboard';\\n        window.xiaoMeiSpeechTimeout = null;\\n\\n        // --- Core Parsing Function (V4 - Abbreviated Tags/Attrs) ---\\n        function parseCustomFormat(dataString) {\\n            if (!dataString) return null;\\n            console.log(\\\"Starting parsing (V4 - Abbreviated)...\\\");\\n            const result = { SPI: {}, SNI_List: [], CMs: {} }; // Adjusted top-level structure for clarity\\n            let currentModuleId = null;\\n            let currentModuleData = null;\\n            const tagRegex = /\\\\[([A-Za-z0-9_]+)(?:\\\\|([^\\\\]]+))?\\\\]/g;\\n            let match;\\n            while ((match = tagRegex.exec(dataString)) !== null) {\\n                const tagName = match[1];\\n                const attributesString = match[2] || '';\\n                const attributes = {};\\n                const attrRegex = /([A-Za-z0-9_]+)=\\\"([^\\\"]*)\\\"/g;\\n                let attrMatch;\\n                while ((attrMatch = attrRegex.exec(attributesString)) !== null) {\\n                    attributes[attrMatch[1]] = attrMatch[2];\\n                }\\n                try {\\n                    if (tagName === 'SPI') { // SystemPanelInfo\\n                        result.SPI = attributes;\\n                        currentActiveTab = attributes.at || 'Dashboard'; // ActiveTab\\n                    } else if (tagName === 'SNI') { // SidebarNavItem\\n                        result.SNI_List.push(attributes);\\n                    } else if (tagName === 'CMS') { // ContentModuleStart\\n                        currentModuleId = attributes.id;\\n                        if (currentModuleId) {\\n                            result.CMs[currentModuleId] = { _meta: attributes };\\n                            currentModuleData = result.CMs[currentModuleId];\\n                        } else { currentModuleId = null; currentModuleData = null; }\\n                    } else if (tagName === 'CME') { // ContentModuleEnd\\n                         if (currentModuleId && attributes.id === currentModuleId) {\\n                            currentModuleId = null; currentModuleData = null;\\n                        }\\n                    } else if (currentModuleData) {\\n                        let listName;\\n                        // Map abbreviated list item tags to their list names in currentModuleData\\n                        if (tagName === 'ELE') listName = 'eles';         // EventLogEntry -> eventLogEntries\\n                        else if (tagName === 'HM') listName = 'hms';          // HaremMember -> haremMembers\\n                        else if (tagName === 'II') listName = 'iis';          // InventoryItem -> inventoryItems\\n                        else if (tagName === 'MLI') listName = 'mlis';        // MallItem -> mallItems\\n                        else if (tagName === 'SLO') listName = 'slos';       // SigninLocation -> signinLocations\\n                        else if (tagName === 'SetI') listName = 'setis';      // SettingItem -> settingItems\\n                        else if (tagName === 'QNB') listName = 'qnbs';        // QuickNavButton -> quickNavButtons\\n                        \\n                        if (listName) {\\n                             if (!currentModuleData[listName]) currentModuleData[listName] = [];\\n                             if (tagName === 'QNBs' && attributes.btns) { // Handle QuickNavigationButtons with 'btns' attribute\\n                                  attributes.btns.split(';').forEach(btnStr => {\\n                                      const [target, textVal] = btnStr.split(':');\\n                                      if (target && textVal) currentModuleData.qnbs.push({ tmid: target, btxt: textVal });\\n                                  });\\n                             } else {\\n                                 currentModuleData[listName].push(attributes);\\n                             }\\n                        }\\n                        // Handle non-list item tags (single objects or direct properties of the module)\\n                        // Ensure these are not misinterpreted system tags from outside a module context\\n                        else if (tagName !== 'SN' && tagName !== 'CM' && tagName !== 'personal_page' && \\n                                 !tagName.endsWith('ListStart') && !tagName.endsWith('ListEnd') && \\n                                 tagName !== 'HS' && tagName !== 'IS' && // These are status tags, not list items themselves\\n                                 tagName !== 'RELS' && tagName !== 'RELE') {\\n                            currentModuleData[tagName] = attributes; // Store as is, or map to a non-abbreviated name if preferred for internal use\\n                        }\\n                    } else if (tagName !== 'SN' && tagName !== 'CM' && tagName !== 'personal_page') {\\n                        console.warn(`Tag ${tagName} found outside module context. Ignored.`);\\n                    }\\n                 } catch (e) {\\n                     console.error(`Error processing tag ${tagName}:`, attributes, e);\\n                 }\\n            }\\n            console.log(\\\"Parsing finished (V4 - Abbreviated):\\\", result);\\n            return result;\\n        }\\n\\n        // --- Core Rendering Functions (Updated for Abbreviated Data) ---\\n        function renderLayout(panelInfo) {\\n            if (!panelInfo) return;\\n            document.title = panelInfo.st || \\\"ç³»ç»Ÿæ¨¡æ¿\\\"; // SystemTitle\\n            document.getElementById('systemTitle').textContent = panelInfo.st || \\\"ç³»ç»Ÿæ¨¡æ¿\\\";\\n            document.getElementById('liuliName').textContent = \\\"{{user}}\\\";\\n\\n            const avatarElement = document.getElementById('liuliAvatar');\\n            if (panelInfo.las && panelInfo.las.trim() !== \\\"\\\") { // LiuliAvatarSrc\\n                 avatarElement.style.backgroundImage = `url('${panelInfo.las}')`;\\n                 avatarElement.style.backgroundColor = 'transparent';\\n            } else {\\n                const placeholder = panelInfo.lap || '?'; // LiuliAvatarPlaceholder\\n                avatarElement.style.backgroundImage = `url('https://via.placeholder.com/80/7b2cbf/e0e0e0?text=${encodeURIComponent(placeholder)}')`;\\n                avatarElement.style.backgroundColor = '#555';\\n            }\\n            avatarElement.title = \\\"{{user}}å¤§äºº\\\";\\n\\n            document.getElementById('footerMessage').innerHTML = panelInfo.fm || \\\"\\\"; // FooterMessage\\n            document.getElementById('xiaoMeiAvatarImg').src = panelInfo.xas || \\\"https://via.placeholder.com/120/4a0063/ff0000?text=?\\\"; // XiaoMeiAvatarSrc\\n            const currentEnergy = parseInt(panelInfo.wce || '0'); // WorldCollectedEnergy\\n            const targetEnergy = parseInt(panelInfo.wte || '0');   // WorldTargetEnergy\\n            const percentage = targetEnergy > 0 ? Math.min((currentEnergy / targetEnergy) * 100, 100) : 0;\\n            document.getElementById('currentCollectedEnergy').textContent = currentEnergy;\\n            document.getElementById('targetTravelEnergy').textContent = targetEnergy;\\n            const energyFill = document.getElementById('energyFill');\\n            energyFill.style.width = percentage + '%';\\n            energyFill.textContent = Math.round(percentage) + '%';\\n            const xiaoMeiTalk = document.getElementById('xiaoMeiTalk');\\n            xiaoMeiTalk.innerHTML = panelInfo.xms || \\\"\\\"; // XiaoMeiSpeech\\n             if (xiaoMeiTalk.innerHTML.trim() !== \\\"\\\") {\\n                 xiaoMeiTalk.style.display = 'block';\\n                 clearTimeout(window.xiaoMeiSpeechTimeout);\\n                 window.xiaoMeiSpeechTimeout = setTimeout(() => {\\n                     if (!document.getElementById('xiaoMeiInteractive')?.matches(':hover')) {\\n                         if(xiaoMeiTalk) xiaoMeiTalk.style.display = 'none';\\n                     }\\n                 }, 7000);\\n            } else {\\n                 xiaoMeiTalk.style.display = 'none';\\n            }\\n        }\\n        function renderSidebar(navItems, activeTabId) {\\n             const navUl = document.getElementById('sidebarNav');\\n             navUl.innerHTML = '';\\n             if (!navItems || !Array.isArray(navItems)) return;\\n             navItems.forEach(item => {\\n                 const li = document.createElement('li');\\n                 const a = document.createElement('a');\\n                 a.href = \\\"#\\\";\\n                 a.dataset.target = item.id; // ID\\n                 a.innerHTML = `<span class=\\\"icon\\\">${item.ic || '?'}</span>${item.tx || 'æœªçŸ¥'}`; // Icon, Text\\n                 if (item.id === activeTabId) { a.classList.add('active'); }\\n                 a.addEventListener('click', handleNavClick);\\n                 li.appendChild(a);\\n                 navUl.appendChild(li);\\n             });\\n        }\\n        function handleNavClick(e) { /* Logic remains the same */\\n            e.preventDefault();\\n            const targetId = this.dataset.target;\\n            if (!parsedSystemData || currentActiveTab === targetId) return;\\n            currentActiveTab = targetId;\\n            const navUl = document.getElementById('sidebarNav');\\n            navUl.querySelectorAll('a').forEach(link => link.classList.toggle('active', link.dataset.target === targetId));\\n            renderContentSection(targetId);\\n            const xiaoMeiTalk = document.getElementById('xiaoMeiTalk');\\n            if (xiaoMeiTalk && xiaoMeiTalk.style.display === 'block' && !document.getElementById('xiaoMeiInteractive')?.matches(':hover')) {\\n                xiaoMeiTalk.style.display = 'none';\\n            }\\n        }\\n        function renderContentSection(sectionId) { /* Logic remains the same for dispatching */\\n             const mainContentArea = document.getElementById('mainContentArea');\\n             mainContentArea.innerHTML = '';\\n             if (!parsedSystemData || !parsedSystemData.CMs || !parsedSystemData.CMs[sectionId]) {\\n                 mainContentArea.innerHTML = `<p class=\\\"placeholder-message\\\">æ— æ³•åŠ è½½æ¨¡å— [${sectionId}] çš„å†…å®¹æ•°æ®ã€‚</p>`;\\n                 return;\\n             }\\n             const moduleData = parsedSystemData.CMs[sectionId];\\n             const section = document.createElement('section');\\n             section.id = sectionId;\\n             section.classList.add('content-section', 'active');\\n             const meta = moduleData._meta || {};\\n             const title = meta.t || sectionId; // Title from CMS tag\\n             const icon = meta.ic || 'â“'; // Icon from CMS tag\\n             const h2 = document.createElement('h2');\\n             h2.innerHTML = `<span class=\\\"icon\\\">${icon}</span>${title}`;\\n             section.appendChild(h2);\\n             switch (sectionId) {\\n                 case 'Dashboard': populateDashboard(section, moduleData); break;\\n                 case 'WorldInfo': populateWorldInfo(section, moduleData); break;\\n                 case 'LiuliProfile': populateLiuliProfile(section, moduleData); break;\\n                 case 'HaremRoster': populateHarem(section, moduleData); break;\\n                 case 'Inventory': populateInventory(section, moduleData); break;\\n                 case 'DemonMall': populateMall(section, moduleData); break;\\n                 case 'FateLottery': populateLottery(section, moduleData); break;\\n                 case 'LocationSignin': populateSignIn(section, moduleData); break;\\n                 case 'SystemSettings': populateSettings(section, moduleData); break;\\n                 default: section.innerHTML += `<p>æœªçŸ¥çš„æ¨¡å—ç±»å‹ã€‚</p>`;\\n             }\\n             mainContentArea.appendChild(section);\\n        }\\n\\n        // --- Section Population Functions (V4 - Updated for Abbreviated Data) ---\\n\\n         function populateDashboard(sectionElement, moduleData) {\\n             const intro = moduleData.MI?.tx || \\\"æ¬¢è¿å›æ¥ï¼\\\"; // ModuleIntro.Text\\n             const summary = moduleData.DWS || {}; // DashboardWorldSummary\\n             const buttons = moduleData.qnbs || []; // QuickNavButtons (parsed as a list of QNB objects)\\n             let buttonsHTML = buttons.map(btnData => {\\n                 if (btnData && btnData.tmid && btnData.btxt) { // TargetModuleID, ButtonText\\n                     return `<button class=\\\"btn\\\" data-nav=\\\"${btnData.tmid}\\\" style=\\\"margin-left:10px;\\\">${btnData.btxt}</button>`;\\n                 } return '';\\n             }).join('');\\n             const outro = moduleData.MO?.tx || \\\"\\\"; // ModuleOutro.Text\\n             sectionElement.innerHTML += `\\n                <div class=\\\"card\\\">\\n                    <h3>${intro}</h3>\\n                    <p>å½“å‰ä¸–ç•Œï¼š<strong>${summary.wn || 'æœªçŸ¥'}</strong> (${summary.df || 'æœªçŸ¥'}éš¾åº¦)</p> <!-- WorldName, Difficulty -->\\n                    <p>${summary.tt || ''}</p> <!-- TipText -->\\n                </div>\\n                <div class=\\\"card\\\">\\n                    <h3>å¿«é€Ÿå¯¼èˆª</h3>\\n                    ${buttonsHTML || '<p>æ— å¿«é€Ÿå¯¼èˆªã€‚</p>'}\\n                </div>\\n                 <p class=\\\"placeholder-message\\\">${outro}</p>\\n            `;\\n            sectionElement.querySelectorAll('.btn[data-nav]').forEach(btn => {\\n                 btn.addEventListener('click', () => {\\n                     const targetNav = document.querySelector(`.system-sidebar a[data-target=\\\"${btn.dataset.nav}\\\"]`);\\n                     if (targetNav) handleNavClick.call(targetNav, { preventDefault: () => {} });\\n                     else console.warn(`Navigation target not found: ${btn.dataset.nav}`);\\n                 });\\n            });\\n        }\\n        function populateWorldInfo(sectionElement, moduleData) {\\n             const intro = moduleData.MI?.tx || \\\"\\\"; // ModuleIntro.Text\\n             const details = moduleData.CWD || {}; // CurrentWorldDetails\\n             const events = moduleData.eles || []; // EventLogEntries\\n             const logTitle = moduleData.RELS?.t || \\\"è¿‘æœŸä¸–ç•ŒåŠ¨æ€\\\"; // RecentEventLogStart.Title\\n             const outro = moduleData.MO?.tx || \\\"\\\"; // ModuleOutro.Text\\n             let charsHTML = (details.mck || \\\"\\\").split(';').map(char => char ? `<li>${char}</li>` : '').join(''); // MainCharactersKnown\\n             if (!charsHTML || details.mck === \\\"æš‚æ— \\\") charsHTML = `<li>æš‚æœªæ¢æ˜</li>`;\\n             let potentialCharsHTML = (details.pmc || \\\"\\\").split(';').map(char => char ? `<li>${char}</li>` : '').join(''); // PotentialMainCharacters\\n             if (!potentialCharsHTML) potentialCharsHTML = `<li>æš‚æ— æƒ…æŠ¥</li>`;\\n             let eventsHTML = events.map(event => `<p><small>${event.ts || ''}</small> ${event.cnt || ''}</p>`).join(''); // Timestamp, Content\\n             sectionElement.innerHTML += `\\n                 <p>${intro}</p>\\n                <div class=\\\"card world-info-current\\\">\\n                    <p><strong>ä¸–ç•Œåç§°ï¼š</strong> ${details.nm || 'æœªçŸ¥'}</p> <!-- Name -->\\n                    <p><strong>ä¸–ç•Œç±»å‹ï¼š</strong> ${details.typ || 'æœªçŸ¥'}</p> <!-- Type -->\\n                    <p><strong>éš¾åº¦è¯„ä¼°ï¼š</strong> ${details.df || 'æœªçŸ¥'}</p> <!-- Difficulty -->\\n                    <p><strong>ä¸–ç•Œç®€ä»‹ï¼š</strong> ${details.bd || 'æš‚æ— '}</p> <!-- BriefDesc -->\\n                    <p><strong>ç›®æ ‡æ„¿åŠ›ï¼š</strong> ${details.twp || 'æœªçŸ¥'} <span class=\\\"currency-icon\\\">ğŸ’–</span></p> <!-- TargetWishPower -->\\n                    <p><strong>å·²æ¢æ˜ä¸»è¦è§’è‰²ï¼š</strong></p>\\n                    <ul>${charsHTML}</ul>\\n                     <p><strong>æ½œåœ¨å…³é”®è§’è‰²ï¼ˆæƒ…æŠ¥ï¼‰ï¼š</strong></p>\\n                    <ul>${potentialCharsHTML}</ul>\\n                </div>\\n                <div class=\\\"card\\\">\\n                    <h3><span class=\\\"icon\\\">ğŸ“œ</span>${logTitle}</h3>\\n                    <div class=\\\"event-log\\\">${eventsHTML || '<p>æš‚æ— ç›¸å…³åŠ¨æ€ã€‚</p>'}</div>\\n                </div>\\n                 <p class=\\\"placeholder-message\\\">${outro}</p>\\n            `;\\n        }\\n        function populateLiuliProfile(sectionElement, moduleData) {\\n            const intro = moduleData.MI?.tx || \\\"\\\"; // ModuleIntro.Text\\n            const attrs = moduleData.LAts || {}; // LiuliAttributes\\n            const skillsData = moduleData.MSL || {}; // MasteredSkillList\\n            const skills = (skillsData.sks || \\\"\\\").split(';').filter(s => s); // Skills\\n            const titlesData = moduleData.TL || {}; // TitleList\\n            const titles = (titlesData.tls || \\\"\\\").split(';').filter(t => t); // Titles\\n            const traitsData = moduleData.LST || {}; // LiuliSpecialTraits\\n            const traits = (traitsData.trs || \\\"\\\").split(';').filter(t => t); // Traits\\n            const outro = moduleData.MO?.tx || \\\"\\\"; // ModuleOutro.Text\\n            const staminaParts = (attrs.sv || \\\"0/100\\\").split('/'); // StaminaValue\\n            const currentStamina = staminaParts[0];\\n            const maxStamina = attrs.ms || (staminaParts.length > 1 ? staminaParts[1] : '100'); // MaxStamina\\n            sectionElement.innerHTML += `\\n                <p>${intro}</p>\\n                <div class=\\\"card liuli-stats\\\">\\n                    <h3>åŸºç¡€å±æ€§</h3>\\n                    <div class=\\\"stat-item\\\"><span class=\\\"stat-label\\\">ç§°å·ï¼š</span><span class=\\\"stat-value\\\">${attrs.t || '-'}</span></div> <!-- Title -->\\n                    <div class=\\\"stat-item\\\"><span class=\\\"stat-label\\\">å½“å‰èº«ä»½ï¼š</span><span class=\\\"stat-value\\\">${attrs.ci || '-'}</span></div> <!-- CurrentIdentity -->\\n                    <div class=\\\"stat-item\\\"><span class=\\\"stat-label\\\">å½“å‰è£…æ‰®ï¼š</span><span class=\\\"stat-value\\\">${attrs.co || '-'}</span></div> <!-- CurrentOutfit -->\\n                    <div class=\\\"stat-item\\\"><span class=\\\"stat-label\\\">é­…åŠ›ï¼š</span><span class=\\\"stat-value\\\">${attrs.cv || '0'}</span></div> <!-- CharmValue -->\\n                    <div class=\\\"stat-item\\\"><span class=\\\"stat-label\\\">ä½“åŠ›ï¼š</span><span class=\\\"stat-value\\\"><span>${currentStamina}</span> / <span>${maxStamina}</span></span></div>\\n                    <div class=\\\"stat-item\\\"><span class=\\\"stat-label\\\">å½“å‰æ€»æ„¿åŠ›ï¼š</span><span class=\\\"stat-value\\\">${attrs.tpwv || '0'}</span></div> <!-- TotalWishPowerValue -->\\n                    <div class=\\\"stat-item\\\"><span class=\\\"stat-label\\\">é­‚åŠ›ï¼š</span><span class=\\\"stat-value\\\">${attrs.slpv || '0'}</span></div> <!-- SoulPowerValue -->\\n                    <div class=\\\"stat-item\\\"><span class=\\\"stat-label\\\">å¥åº·çŠ¶å†µï¼š</span><span class=\\\"stat-value\\\">${attrs.chs || 'æœªçŸ¥'}</span></div> <!-- CurrentHealthStatus -->\\n                </div>\\n                <div class=\\\"card\\\">\\n                    <h3>${skillsData.t || 'å·²æŒæ¡æŠ€èƒ½/å¤©èµ‹'}</h3> <!-- Title -->\\n                    <p>${skills.length > 0 ? skills.join(', ') : \\\"æš‚æ— \\\"}</p>\\n                </div>\\n                <div class=\\\"card\\\">\\n                    <h3>${titlesData.t || 'å·²è·å¾—ç§°å·'}</h3> <!-- Title -->\\n                    <p>${titles.length > 0 ? titles.join(', ') : \\\"æš‚æ— \\\"}</p>\\n                </div>\\n                <div class=\\\"card\\\">\\n                    <h3>${traitsData.t || 'ç‰¹æ®Šç‰¹è´¨'}</h3> <!-- Title (if LST has one) or fallback -->\\n                    <p>${traits.length > 0 ? traits.join('; ') : \\\"æš‚æ— \\\"}</p>\\n                </div>\\n                <p class=\\\"placeholder-message\\\">${outro}</p>\\n            `;\\n        }\\n       function populateHarem(sectionElement, moduleData) {\\n            const intro = moduleData.MI?.tx || \\\"\\\"; // ModuleIntro.Text\\n            const status = moduleData.HS; // HaremStatus\\n            const members = moduleData.hms || []; // HaremMembers (list)\\n            const outro = moduleData.MO?.tx || \\\"\\\"; // ModuleOutro.Text\\n\\n            sectionElement.innerHTML += `\\n                <p>${intro}</p>\\n                <div class=\\\"harem-gallery\\\" id=\\\"haremGalleryContainer\\\"></div>\\n                <p class=\\\"placeholder-message\\\">${outro}</p>\\n            `;\\n            const container = sectionElement.querySelector('#haremGalleryContainer');\\n\\n            if (members.length === 0) {\\n                const placeholderText = status?.pht || 'æš‚æ— å€¾æ…•ä¹‹äººä¿¡æ¯ã€‚'; // PlaceholderText\\n                container.innerHTML = `<div class=\\\"harem-placeholder\\\">${placeholderText}</div>`;\\n                if (status?.ie === \\\"false\\\") { // IsEmpty\\n                    console.warn(\\\"HaremStatus indicates non-empty, but HaremMembers list (hms) is empty.\\\");\\n                }\\n                return;\\n            }\\n\\n            members.forEach(member => {\\n                 const card = document.createElement('div');\\n                 card.classList.add('harem-member-card');\\n                 let seed = 0;\\n                 const seedChar = member.as || '?'; // AvatarSeed\\n                 for (let i = 0; i < seedChar.length; i++) { seed = seedChar.charCodeAt(i) + ((seed << 5) - seed); }\\n                 const color = (seed & 0x00FFFFFF).toString(16).toUpperCase();\\n                 const avatarColor = \\\"000000\\\".substring(0, 6 - color.length) + color;\\n                 const interactions = (member.io || \\\"\\\").split(';').filter(opt => opt); // InteractionOptions\\n                 card.innerHTML = `\\n                     <div class=\\\"harem-avatar\\\" style=\\\"background-image: url('https://via.placeholder.com/80/${avatarColor}/FFFFFF?text=${encodeURIComponent(seedChar)}');\\\"></div>\\n                     <h4 class=\\\"harem-member-name\\\">${member.nm || 'æœªçŸ¥'}</h4> <!-- Name -->\\n                     <p class=\\\"harem-member-status\\\">ç¾ç»Š: ${member.bln || '-'}</p> <!-- BondLevelName -->\\n                     <p class=\\\"harem-member-status\\\">å¿ƒæƒ…: ${member.cmd || '-'}</p> <!-- CurrentMood -->\\n                     <p class=\\\"harem-member-status\\\">æ„¿åŠ›: ${member.dwo || '0'} <span class=\\\"currency-icon\\\">ğŸ’–</span>/æ—¥</p> <!-- DailyWishOutput -->\\n                     ${member.ss ? `<p class=\\\"harem-member-status\\\" style=\\\"color:#ff7070;\\\">çŠ¶æ€: ${member.ss}</p>` : ''} <!-- SpecialStatus -->\\n                     ${interactions.length > 0 ? `<div style=\\\"margin-top:10px;\\\">${interactions.map(opt => `<button class=\\\"btn btn-secondary btn-small\\\" data-harem-action=\\\"${member.mid}:${opt}\\\">${opt}</button>`).join(' ')}</div>` : ''} <!-- MemberID -->\\n                 `;\\n                 container.appendChild(card);\\n                 card.querySelectorAll('button[data-harem-action]').forEach(button => {\\n                     button.addEventListener('click', (e) => {\\n                         const actionData = e.currentTarget.dataset.haremAction.split(':');\\n                         const memberId = actionData[0];\\n                         const action = actionData[1];\\n                         console.log(`Harem action: ${action} on ${memberId}`);\\n                         xiaoMeiSpeak(`è¦å¯¹ã€${member.nm}ã€‘æ‰§è¡Œ[${action}]äº’åŠ¨å—ï¼Œ{{user}}ï¼Ÿ`);\\n                     });\\n                 });\\n            });\\n        }\\n        function populateInventory(sectionElement, moduleData) {\\n            const intro = moduleData.MI?.tx || \\\"\\\"; // ModuleIntro.Text\\n            const status = moduleData.IS; // InventoryStatus\\n            const items = moduleData.iis || []; // InventoryItems (list)\\n            const outro = moduleData.MO?.tx || \\\"\\\"; // ModuleOutro.Text\\n\\n            sectionElement.innerHTML += `\\n                <p>${intro}</p>\\n                <div class=\\\"inventory-grid\\\" id=\\\"inventoryItemContainer\\\"></div>\\n                <p class=\\\"placeholder-message\\\">${outro}</p>\\n            `;\\n            const container = sectionElement.querySelector('#inventoryItemContainer');\\n\\n            if (items.length === 0) {\\n                const placeholderText = status?.pht || 'è¡Œå›Šç©ºç©ºå¦‚ä¹Ÿã€‚'; // PlaceholderText\\n                container.innerHTML = `<div class=\\\"inventory-item-placeholder\\\">${placeholderText}</div>`;\\n                 if (status?.ie === \\\"false\\\") { // IsEmpty\\n                    console.warn(\\\"InventoryStatus indicates non-empty, but InventoryItems list (iis) is empty.\\\");\\n                }\\n                return;\\n            }\\n\\n            items.forEach(item => {\\n                 const itemDiv = document.createElement('div');\\n                 itemDiv.classList.add('inventory-item');\\n                 itemDiv.setAttribute('data-item-instance-id', item.iid); // ItemInstanceID\\n                 let actionButtonsHTML = '';\\n                 if (item.iu === 'true') { // IsUsable\\n                     actionButtonsHTML += `<button class=\\\"btn btn-secondary btn-small\\\" data-action=\\\"use\\\" data-item-id=\\\"${item.iid}\\\">ä½¿ç”¨</button>`;\\n                 }\\n                 if (item.ieq === 'true') { // IsEquippable\\n                     actionButtonsHTML += `<button class=\\\"btn btn-secondary btn-small\\\" data-action=\\\"equip\\\" data-item-id=\\\"${item.iid}\\\">è£…å¤‡</button>`;\\n                 }\\n                 itemDiv.innerHTML = `\\n                     <div class=\\\"inventory-item-icon\\\">${item.ic || 'ğŸ“¦'}</div> <!-- Icon -->\\n                     <div class=\\\"inventory-item-name\\\">${item.nm || 'æœªçŸ¥ç‰©å“'}</div> <!-- Name -->\\n                     <div class=\\\"inventory-item-quantity\\\">æ•°é‡: ${item.qty || '1'}</div> <!-- Quantity -->\\n                     <div class=\\\"inventory-item-description\\\">${item.dsc || 'æ— æè¿°'} (${item.typ || 'æœªçŸ¥ç±»å‹'})<br/>${item.es ? `æ•ˆæœ: ${item.es}`: ''}</div> <!-- Description, Type, EffectSummary -->\\n                     <div class=\\\"inventory-item-actions\\\">${actionButtonsHTML}</div>\\n                 `;\\n                 itemDiv.addEventListener('click', handleInventoryItemClick);\\n                 itemDiv.querySelectorAll('.inventory-item-actions button').forEach(button => {\\n                     button.addEventListener('click', (e) => {\\n                         e.stopPropagation();\\n                         const action = e.currentTarget.dataset.action;\\n                         const itemId = e.currentTarget.dataset.itemId;\\n                         console.log(`Action: ${action}, Item: ${itemId}`);\\n                         xiaoMeiSpeak(`{{user}}è¦å¯¹ã€${item.nm}ã€‘æ‰§è¡Œ[${action}]æ“ä½œå—ï¼Ÿ`);\\n                     });\\n                 });\\n                 container.appendChild(itemDiv);\\n            });\\n        }\\n        function populateMall(sectionElement, moduleData) {\\n            const intro = moduleData.MI?.tx || \\\"\\\"; // ModuleIntro.Text\\n            const outro = moduleData.MO?.tx || \\\"\\\"; // ModuleOutro.Text\\n            const items = moduleData.mlis || []; // MallItems (list)\\n            const refreshInfo = moduleData.MRI?.tx || \\\"\\\"; // MallRefreshInfo.Text\\n            sectionElement.innerHTML += `\\n                <p class=\\\"section-intro\\\">${intro}</p>\\n                <div class=\\\"mall-item-grid\\\" id=\\\"mallItemContainer\\\"></div>\\n                ${refreshInfo ? `<p class=\\\"section-outro\\\">${refreshInfo}</p>` : ''}\\n                <p class=\\\"placeholder-message\\\">${outro}</p>\\n            `;\\n            const container = sectionElement.querySelector('#mallItemContainer');\\n            if (items.length === 0) {\\n                container.innerHTML = `<p class=\\\"placeholder-message\\\">å•†åº—ç›®å‰ç©ºç©ºå¦‚ä¹Ÿã€‚</p>`;\\n                return;\\n            }\\n            items.forEach(item => {\\n                const itemDiv = document.createElement('div');\\n                itemDiv.classList.add('mall-item');\\n                let stockDisplay = item.stk; // Stock\\n                if (stockDisplay === '0' && item.wlc !== '-1') stockDisplay = 'ç½„'; // WorldLimitCount\\n                else if (stockDisplay === '-1') stockDisplay = 'âˆ';\\n                let limitText = \\\"\\\";\\n                const limit = parseInt(item.wlc || '-1');\\n                const purchased = parseInt(item.pwc || '0'); // PurchasedInWorldCount\\n                if (limit !== -1) { limitText = ` (æœ¬ä¸–ç•Œå·²è´­ ${purchased}/${limit})`; }\\n                else if (purchased > 0) { limitText = ` (æœ¬ä¸–ç•Œå·²è´­ ${purchased})`; }\\n                let fullDesc = item.dsc || 'æ— æè¿°'; // Description\\n                if (item.pif) fullDesc += `<br><span class=\\\"promo\\\">${item.pif}</span>`; // PromoInfo\\n                if (item.pcd) fullDesc += `<br><span class=\\\"condition\\\">${item.pcd}</span>`; // PurchaseCondition\\n                if (limitText) fullDesc += `<span class=\\\"limit\\\">${limitText}</span>`;\\n                const currencyIcon = item.ctyp === 'é­”æ™¶' ? 'ğŸ’' : (item.ic_cur || 'ğŸ’–'); // CurrencyType, CurrencyIcon (assuming ic_cur if provided)\\n                itemDiv.innerHTML = `\\n                    <div class=\\\"mall-item-details\\\">\\n                        <h4 class=\\\"mall-item-name\\\">${item.ic || 'ğŸ›’'} ${item.nm || 'æœªçŸ¥å•†å“'} <span style=\\\"font-size:12px; color:#888;\\\">(åº“å­˜: ${stockDisplay})</span></h4> <!-- Icon, Name -->\\n                        <p class=\\\"mall-item-desc\\\">${fullDesc}</p>\\n                    </div>\\n                    <div class=\\\"mall-item-footer\\\">\\n                        <span class=\\\"mall-item-price\\\"><span class=\\\"currency-icon\\\">${currencyIcon}</span> ${item.prc || '0'}</span> <!-- Price -->\\n                        <button class=\\\"btn btn-secondary buy-item-btn\\\" data-item-id=\\\"${item.itid}\\\" ${item.ibd === 'true' ? 'disabled' : ''}>\\n                            ${item.btxt || 'è´­ä¹°'} <!-- ItemID, IsButtonDisabled, ButtonText -->\\n                        </button>\\n                    </div>\\n                `;\\n                container.appendChild(itemDiv);\\n            });\\n            sectionElement.querySelectorAll('.buy-item-btn').forEach(button => {\\n                button.addEventListener('click', (e) => {\\n                    const itemId = e.currentTarget.getAttribute('data-item-id');\\n                    const itemElement = e.currentTarget.closest('.mall-item');\\n                    const nameElement = itemElement ? itemElement.querySelector('.mall-item-name') : null;\\n                    let itemName = itemId;\\n                     if (nameElement) {\\n                        const nameText = nameElement.textContent || \\\"\\\";\\n                         itemName = nameText.replace(/^.*? /,'').split(' (åº“å­˜:')[0].trim();\\n                    }\\n                    console.log(`Attempting to buy item: ${itemId} (${itemName})`);\\n                    xiaoMeiSpeak(`{{user}}æƒ³è¦è´­ä¹°ã€${itemName}ã€‘å—ï¼Ÿè¯·æŒ‡ç¤ºã€‚`);\\n                });\\n            });\\n        }\\n        function populateLottery(sectionElement, moduleData) {\\n            const intro = moduleData.MI?.tx || \\\"è¯•è¯•æ‰‹æ°”å§ï¼\\\"; // ModuleIntro.Text\\n            const info = moduleData.LI || {}; // LotteryInfo\\n            const button = moduleData.LB || {}; // LotteryButton\\n            const rareCountData = moduleData.RRC || {}; // RareRewardCounter\\n            const previewData = moduleData.PPP || {}; // PrizePoolPreview\\n            const preview = (previewData.pzs || \\\"\\\").split(';').filter(p => p); // Prizes\\n            const outro = moduleData.MO?.tx || \\\"\\\"; // ModuleOutro.Text\\n            const currencyIcon = info.cur === 'é­”æ™¶' ? 'ğŸ’' : (info.cur === 'ç²¾çº¯æ„¿åŠ›' ? 'ğŸ’™' : 'ğŸ’–'); // Currency\\n            sectionElement.innerHTML += `\\n                <div class=\\\"card lottery-area\\\">\\n                    <h3>${intro}</h3>\\n                    <p class=\\\"lottery-info\\\">\\n                        æ¯æ¬¡æŠ½å¥–æ¶ˆè€— <strong style='color:#ffd700;'>${info.cst || '?'} <span class='currency-icon'>${currencyIcon}</span> ${info.cur || 'æ„¿åŠ›'}</strong>ã€‚\\n                    </p>\\n                    <div class=\\\"lottery-wheel-placeholder\\\">${info.wph || 'è½®ç›˜'}</div> <!-- WheelPlaceholder -->\\n                    <button class=\\\"btn\\\" id=\\\"lotteryButton\\\" ${button.idb === 'true' ? 'disabled' : ''}>${button.tx || 'æŠ½å¥–'}</button> <!-- IsDisabled, Text -->\\n                    <div class=\\\"lottery-result\\\" id=\\\"lotteryResultDisplay\\\">${info.lr && info.lr !== \\\"å°šæœªè½¬åŠ¨\\\" ? 'ä¸Šæ¬¡ç»“æœï¼š'+info.lr : ''}</div> <!-- LastResult -->\\n                    <p class=\\\"lottery-info\\\" style=\\\"font-size:12px; margin-top:15px;\\\">\\n                         ${rareCountData.t || 'ç¨€æœ‰å¥–åŠ±å‰©ä½™'}ï¼š<span id='rareRewardsLeft'>${rareCountData.cntv || 'æœªçŸ¥'}</span>ä»½ <!-- Title, CountValue -->\\n                    </p>\\n                     <p class=\\\"lottery-info\\\" style=\\\"font-size:11px; color:#a0a0c0;\\\">${previewData.t || 'å¥–æ± é¢„è§ˆ'}: ${preview.length > 0 ? preview.join('; ') : 'æš‚æ— '} </p> <!-- Title -->\\n                </div>\\n                 <p class=\\\"placeholder-message\\\">${outro}</p>\\n            `;\\n            sectionElement.querySelector('#lotteryButton').addEventListener('click', () => {\\n                 console.log(\\\"Lottery button clicked\\\");\\n                 xiaoMeiSpeak(`{{user}}è¦è¯•è¯•è¿æ°”å—ï¼Ÿè¯·ç¡®è®¤ã€‚`);\\n            });\\n        }\\n        function populateSignIn(sectionElement, moduleData) {\\n            const intro = moduleData.MI?.tx || \\\"\\\"; // ModuleIntro.Text\\n            const listInfo = moduleData.SL || {}; // SigninList\\n            const locations = moduleData.slos || []; // SigninLocations (list)\\n            const statusMsg = moduleData.SSM?.lr || \\\"\\\"; // SigninStatusMessage.LastResult\\n            const outro = moduleData.MO?.tx || \\\"\\\"; // ModuleOutro.Text\\n            sectionElement.innerHTML += `\\n                <div class=\\\"card\\\">\\n                    <h3>${intro}</h3>\\n                    <p>é¦–æ¬¡æŠµè¾¾ç‰¹å®šå‰§æƒ…åœ°ç‚¹å³å¯ç­¾åˆ°ã€‚</p>\\n                     <p style=\\\"font-size:12px; color:#a0a0c0; text-align:center;\\\">å½“å‰ä¸–ç•Œ: ${listInfo.cw || 'æœªçŸ¥'}</p> <!-- CurrentWorld -->\\n                    <ul class=\\\"signin-location-list\\\" id=\\\"signinLocationList\\\"></ul>\\n                    <p class=\\\"signin-status-message\\\" id=\\\"signInStatusMessage\\\">${statusMsg ? 'ä¸Šæ¬¡ç­¾åˆ°ï¼š'+statusMsg : ''}</p>\\n                </div>\\n                <p class=\\\"placeholder-message\\\">${outro}</p>\\n            `;\\n            const list = sectionElement.querySelector('#signinLocationList');\\n            if (listInfo.ie === \\\"true\\\" || locations.length === 0) { // IsEmpty\\n                list.innerHTML = `<li class=\\\"placeholder-message\\\">${listInfo.pht || 'å½“å‰ä¸–ç•Œæ— ç­¾åˆ°ç‚¹ã€‚'}</li>`; // PlaceholderText\\n                return;\\n            }\\n            locations.forEach(loc => {\\n                const item = document.createElement('li');\\n                item.classList.add('signin-location-item');\\n                const isButtonDisabled = loc.isg === 'true' || loc.ibd === 'true'; // IsSigned, IsButtonDisabled\\n                item.innerHTML = `\\n                    <div class=\\\"signin-location-details\\\">\\n                        <span class=\\\"signin-location-name\\\">${loc.nm || 'æœªçŸ¥åœ°ç‚¹'} ${loc.isg === 'true' ? '(å·²ç­¾åˆ°)' : ''}</span> <!-- Name -->\\n                        <p class=\\\"signin-location-reward\\\">${loc.isg === 'true' ? `å·²é¢†å–å¥–åŠ±` : `é¢„è®¡å¥–åŠ±: ${loc.rp || '???'}`}</p> <!-- RewardPreview -->\\n                        ${loc.cnd ? `<p style=\\\"font-size:11px; color:#ff9999;\\\">æ¡ä»¶: ${loc.cnd}</p>` : ''} <!-- Condition -->\\n                    </div>\\n                    <button class=\\\"btn ${isButtonDisabled ? 'disabled' : 'btn-secondary'} sign-in-btn\\\" data-loc-id=\\\"${loc.lid}\\\" title=\\\"${loc.cnd || ''}\\\">\\n                        ${loc.btxt || 'ç­¾åˆ°'} <!-- LocationID, ButtonText -->\\n                    </button>\\n                `;\\n                list.appendChild(item);\\n            });\\n            sectionElement.querySelectorAll('.sign-in-btn:not([disabled])').forEach(button => {\\n                button.addEventListener('click', (e) => {\\n                    const locId = e.currentTarget.getAttribute('data-loc-id');\\n                    const locElement = e.currentTarget.closest('.signin-location-item');\\n                    const locName = locElement ? locElement.querySelector('.signin-location-name').textContent.split('(')[0].trim() : locId;\\n                    console.log(`Attempting sign in at: ${locId}`);\\n                    xiaoMeiSpeak(`{{user}}è¦åœ¨æ­¤å¤„ã€${locName}ã€‘ç•™ä¸‹å°è®°å—ï¼Ÿ`);\\n                });\\n            });\\n        }\\n        function populateSettings(sectionElement, moduleData) {\\n            const intro = moduleData.MI?.tx || \\\"\\\"; // ModuleIntro.Text\\n            const items = moduleData.setis || []; // SettingItems (list)\\n            const outro = moduleData.MO?.tx || \\\"\\\"; // ModuleOutro.Text\\n            sectionElement.innerHTML += `<p>${intro}</p>`;\\n            if (items.length === 0) {\\n                 sectionElement.innerHTML += `<p class=\\\"placeholder-message\\\">æš‚æ— è®¾ç½®é¡¹ã€‚</p>`;\\n            } else {\\n                 items.forEach(item => {\\n                     const card = document.createElement('div');\\n                     card.classList.add('card');\\n                     let controlHTML = '';\\n                     card.innerHTML = `<h3>${item.lbl || 'è®¾ç½®é¡¹'}</h3>`; // Label\\n                     if (item.typ === 'Slider') { // Type\\n                         controlHTML = `<input type=\\\"range\\\" min=\\\"${item.min || '0'}\\\" max=\\\"${item.max || '100'}\\\" step=\\\"${item.stp || '1'}\\\" value=\\\"${item.val || '50'}\\\" style=\\\"width:100%; cursor:pointer;\\\" data-setting-id=\\\"${item.id}\\\">`; // Min, Max, Step, Value, ID\\n                     } else if (item.typ === 'ButtonGroup') {\\n                         const options = (item.opts || \\\"\\\").split(';').filter(opt => opt); // Options\\n                         const selectedValue = item.sel || \\\"\\\"; // Selected\\n                         controlHTML = options.map(opt => {\\n                             const optValue = opt.replace(/\\\\(.*\\\\)/, '').trim();\\n                             const isSelected = selectedValue === optValue;\\n                             return `<button class=\\\"btn btn-secondary ${isSelected ? 'active' : ''}\\\" data-setting-id=\\\"${item.id}\\\" data-value=\\\"${optValue}\\\" style=\\\"margin-right: 5px;\\\" title=\\\"${opt}\\\">${opt}</button>`;\\n                         }).join(' ');\\n                     } else if (item.typ === 'Switch') {\\n                          const isChecked = item.cvl === 'true'; // CurrentValue\\n                          controlHTML = `\\n                              <label class=\\\"switch\\\">\\n                                  <input type=\\\"checkbox\\\" data-setting-id=\\\"${item.id}\\\" ${isChecked ? 'checked' : ''}>\\n                                  <span class=\\\"slider round\\\"></span>\\n                              </label>\\n                          `;\\n                     }\\n                     card.innerHTML += `\\n                         <div style=\\\"margin-bottom: 10px;\\\">${controlHTML}</div>\\n                         <p style=\\\"font-size: 12px; color: #a0a0c0;\\\">${item.dsc || ''}</p> <!-- Description -->\\n                     `;\\n                     sectionElement.appendChild(card);\\n                 });\\n                sectionElement.querySelectorAll('input[type=\\\"range\\\"], input[type=\\\"checkbox\\\"], button[data-setting-id]').forEach(el => {\\n                     if (el.tagName === 'INPUT' && el.type === 'range') {\\n                          el.addEventListener('change', (e) => {\\n                             const settingId = e.target.dataset.settingId;\\n                             const newValue = e.target.value;\\n                             console.log(`Setting changed (final): ${settingId} = ${newValue}`);\\n                             xiaoMeiSpeak(`{{user}}è°ƒæ•´äº† [${el.closest('.card').querySelector('h3').textContent}] è®¾ç½®ã€‚`);\\n                         });\\n                     } else if (el.tagName === 'INPUT' && el.type === 'checkbox') {\\n                          el.addEventListener('change', (e) => {\\n                             const settingId = e.target.dataset.settingId;\\n                             const newValue = e.target.checked;\\n                             console.log(`Setting changed: ${settingId} = ${newValue}`);\\n                              xiaoMeiSpeak(`{{user}}è°ƒæ•´äº† [${el.closest('.card').querySelector('h3').textContent}] è®¾ç½®ã€‚`);\\n                         });\\n                     } else if (el.tagName === 'BUTTON') {\\n                         el.addEventListener('click', (e) => {\\n                              if (e.target.classList.contains('active')) return;\\n                              const settingId = e.target.dataset.settingId;\\n                              const newValue = e.target.dataset.value;\\n                              const label = e.target.closest('.card').querySelector('h3').textContent;\\n                              console.log(`Setting selected: ${settingId} = ${newValue}`);\\n                              xiaoMeiSpeak(`{{user}}å°† [${label}] è®¾ç½®ä¸º [${newValue}] äº†ã€‚`);\\n                              el.closest('.card').querySelectorAll('button[data-setting-id=\\\"' + settingId + '\\\"]').forEach(btn => {\\n                                   btn.classList.remove('active');\\n                              });\\n                              e.target.classList.add('active');\\n                         });\\n                     }\\n                 });\\n            }\\n             sectionElement.innerHTML += `<p class=\\\"placeholder-message\\\">${outro}</p>`;\\n        }\\n\\n        // --- UI Update & Helper Functions ---\\n        function handleInventoryItemClick(event) { /* Logic remains the same */\\n             const itemDiv = event.currentTarget;\\n             if (event.target.tagName === 'BUTTON') return;\\n             const descriptionDiv = itemDiv.querySelector('.inventory-item-description');\\n             if (descriptionDiv) {\\n                 const isActive = descriptionDiv.style.display === 'block';\\n                 const parentGrid = itemDiv.closest('.inventory-grid');\\n                 if (parentGrid) {\\n                     parentGrid.querySelectorAll('.inventory-item-description').forEach(desc => {\\n                         if (desc !== descriptionDiv) desc.style.display = 'none';\\n                     });\\n                 }\\n                 descriptionDiv.style.display = isActive ? 'none' : 'block';\\n             }\\n        }\\n        function xiaoMeiSpeak(customMessage = null) {\\n             const xiaoMeiTalk = document.getElementById('xiaoMeiTalk');\\n             if (!xiaoMeiTalk) return;\\n             if (customMessage) {\\n                 const userName = parsedSystemData?.SPI?.ln || 'æ‚¨'; // SystemPanelInfo.LiuliName\\n                 const finalMessage = customMessage.replace(/\\\\{\\\\{user\\\\}\\\\}/g, userName);\\n                 xiaoMeiTalk.innerHTML = finalMessage;\\n                 xiaoMeiTalk.style.display = 'block';\\n                 clearTimeout(window.xiaoMeiSpeechTimeout);\\n                 window.xiaoMeiSpeechTimeout = setTimeout(() => {\\n                     if (!document.getElementById('xiaoMeiInteractive')?.matches(':hover')) {\\n                        if (xiaoMeiTalk) xiaoMeiTalk.style.display = 'none';\\n                    }\\n                }, 7000);\\n             }\\n        }\\n\\n        // --- Initialization and Update Trigger ---\\n        function initializeOrUpdateUI(dataString) {\\n            try {\\n                parsedSystemData = parseCustomFormat(dataString);\\n                if (!parsedSystemData || !parsedSystemData.SPI) { // Check for SPI (SystemPanelInfo)\\n                    throw new Error(\\\"æ— æ³•è§£ææ•°æ®æˆ–ç¼ºå°‘ SPI (ç³»ç»Ÿé¢æ¿ä¿¡æ¯)ã€‚\\\");\\n                }\\n                console.log(\\\"Parsed Data (Abbreviated):\\\", parsedSystemData);\\n                currentActiveTab = parsedSystemData.SPI.at || 'Dashboard'; // ActiveTab from SPI\\n                renderLayout(parsedSystemData.SPI);\\n                renderSidebar(parsedSystemData.SNI_List, currentActiveTab); // SidebarNavItems list\\n                renderContentSection(currentActiveTab);\\n                setupXiaoMeiInteraction();\\n            } catch (error) {\\n                 console.error(\\\"UIåˆå§‹åŒ–/æ›´æ–°é”™è¯¯:\\\", error);\\n                 const mainArea = document.getElementById('mainContentArea');\\n                 if(mainArea) mainArea.innerHTML = `<p class=\\\"placeholder-message\\\" style=\\\"color: red;\\\">é”™è¯¯ï¼šæ¸²æŸ“ç•Œé¢æ—¶å‡ºé”™ï¼<br>${error.message}</p>`;\\n            }\\n        }\\n        function setupXiaoMeiInteraction() { /* Logic remains the same */\\n             const xiaoMeiInteractive = document.getElementById('xiaoMeiInteractive');\\n             if (xiaoMeiInteractive) {\\n                 const new_el = xiaoMeiInteractive.cloneNode(true);\\n                 const parent = xiaoMeiInteractive.parentNode;\\n                 if(parent) parent.replaceChild(new_el, xiaoMeiInteractive);\\n                 new_el.addEventListener('click', () => {\\n                     console.log(\\\"Xiao Mei clicked\\\");\\n                     const userName = parsedSystemData?.SPI?.ln || 'æ‚¨'; // LiuliName from SPI\\n                     xiaoMeiSpeak(`${userName}æœ‰ä½•å©å’ï¼Ÿ`);\\n                 });\\n               