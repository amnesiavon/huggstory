            hideMusicPlayer();\\n                        });\\n                    } else {\\n                        // æš‚åœéŸ³é¢‘\\n                        audioElement.pause();\\n                        icon.classList.remove('fa-pause');\\n                        icon.classList.add('fa-play');\\n                        hideMusicPlayer();\\n                    }\\n                });\\n            } else if (type === 'link') {\\n                messageDiv.classList.add('link');\\n                let iconUrl;\\n                if (sourceType === 'è®ºå›') {\\n                    iconUrl = 'https://files.catbox.moe/6z67cv.png';\\n                } else if (sourceType === 'æœ‹å‹åœˆ') {\\n                    iconUrl = 'https://files.catbox.moe/wgc908.png';\\n                } else if (sourceType === 'è§†é¢‘') {\\n                    iconUrl = 'https://files.catbox.moe/uo44tm.png';\\n                } else if (sourceType === 'è½¯ä»¶' || sourceType === 'æ·»åŠ å¥½å‹' || sourceType === 'åŠ å…¥ç¾¤èŠ') {\\n                    iconUrl = 'https://files.catbox.moe/cnc4uh.png';\\n                }\\n\\n                if (sourceType === 'é¥­å¦å¤–å–' || sourceType === 'è¯†è´§ç™¾ç‰©') {\\n                    messageDiv.classList.add('menu-card');\\n                    const menuItems = text.split('ã€');\\n                    messageDiv.innerHTML = `\\n                        <div class=\\\"menu-card-header\\\">\\n                            <div class=\\\"menu-card-icon\\\">\\n                                <img src=\\\"${sourceType === 'é¥­å¦å¤–å–' ? 'https://files.catbox.moe/xlvot3.JPG' : 'https://files.catbox.moe/oi30mp.png'}\\\" alt=\\\"${sourceType}\\\">\\n                            </div>\\n                            <div class=\\\"menu-card-title\\\" style=\\\"color: #000; font-weight: bold; text-align: center;\\\">${sourceType}</div>\\n                        </div>\\n                        <div class=\\\"menu-items-list\\\">\\n                            ${menuItems.