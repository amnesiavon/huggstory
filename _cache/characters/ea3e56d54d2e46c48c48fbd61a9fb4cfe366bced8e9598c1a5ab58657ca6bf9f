            const overlay = document.querySelector('.overlay');\\n            confirmDialog.style.display = 'none';\\n            overlay.style.display = 'none';\\n        }\\n\\n        // æ›´æ–°æ—¶é—´æ˜¾ç¤º\\n        function updateTime() {\\n            const timeElement = document.getElementById('time');\\n            const now = new Date();\\n            const hours = now.getHours().toString().padStart(2, '0');\\n            const minutes = now.getMinutes().toString().padStart(2, '0');\\n            timeElement.textContent = `${hours}:${minutes}`;\\n            \\n            setTimeout(updateTime, CONFIG.UPDATE_INTERVAL);\\n        }\\n\\n        // åˆå§‹åŒ–\\n        document.addEventListener('DOMContentLoaded', () => {\\n            const posts = parseForumContent(forumContent);\\n            \\n            // æ¸²æŸ“è®ºå›å†…å®¹\\n            const forumContainer = document.querySelector('.forum-container');\\n            posts.forEach(post => {\\n                const postItem = document.createElement('div');\\n                postItem.className = 'post-item';\\n                \\n                // ä¸å†æˆªæ–­å†…å®¹ï¼Œæ˜¾ç¤ºå®Œæ•´å†…å®¹\\n                postItem.innerHTML = `\\n                    <div class=\\\"post-header\\\">\\n                        <div class=\\\"post-title\\\">${post.title}</div>\\n                    </div>\\n                    <div class=\\\"post-author\\\">${post.author}</div>\\n                    <div class=\\\"post-content\\\">${post.content}</div>\\n                `;\\n                \\n                postItem.addEventListener('click', () => {\\n                    if (post.comments && post.comments.length > 0) {\\n                        // å…ˆæ˜¾ç¤ºç¡®è®¤å¯¹è¯æ¡†\\n                        showConfirmDialog(post.title, () => {\\n                            if (typeof triggerSlash === 'function') {\\n                                triggerSlash(`/send è¿›å…¥è®ºå›è¯¦æƒ…ç•Œé¢ï¼š${post.title}|/trigger`);\\n                   