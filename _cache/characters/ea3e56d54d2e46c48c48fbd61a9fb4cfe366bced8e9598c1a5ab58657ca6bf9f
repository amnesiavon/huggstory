           // æ­£åˆ™è¡¨è¾¾å¼\\n            REGEX: {\\n                FOOD_INFO: /\\\\{(.*?)\\\\|(.*?)\\\\|(.*?)\\\\|(.*?)\\\\|(.*?)\\\\|(.*?)\\\\}/,\\n                MESSAGE_INFO: /\\\\{(.*?)\\\\|(.*?)\\\\|(.*?)\\\\|(.*?)\\\\}/\\n            }\\n        };\\n\\n\\n        // æµ‹è¯•æ•°æ®\\n        const TEST_CONTENT = `$1`;\\n        // è§£æå†…å®¹\\n        function parseContent(text) {\\n            const result = {\\n                food: null,\\n                message: null\\n            };\\n            \\n            const matches = text.match(/\\\\{.*?\\\\}/g);\\n            if (matches) {\\n                // è§£æé£Ÿå“ä¿¡æ¯\\n                const foodMatch = matches[0].match(CONFIG.REGEX.FOOD_INFO);\\n                if (foodMatch) {\\n                    result.food = {\\n                        name: foodMatch[1],\\n                        description: foodMatch[2],\\n                        shop: foodMatch[3],\\n                        rating: foodMatch[4],\\n                        price: foodMatch[5],\\n                        time: foodMatch[6]\\n                    };\\n                }\\n                \\n                // è§£ææ¶ˆæ¯ä¿¡æ¯\\n                if (matches[1]) {\\n                    const messageMatch = matches[1].match(CONFIG.REGEX.MESSAGE_INFO);\\n                    if (messageMatch) {\\n                        result.message = {\\n                            name: messageMatch[1],\\n                            avatar: `https://files.catbox.moe/${messageMatch[2]}`,\\n                            text: messageMatch[3],\\n                            time: messageMatch[4]\\n                        };\\n                    }\\n                }\\n            }\\n            \\n            return result;\\n        }\\n\\n        // åˆå§‹åŒ–\\n        document.addEventListener('DOMContentLoaded', () => {\\n            // è§£ææµ‹è¯•å†…å®¹\\n            const content = parseContent(TEST_CONTENT);\\n            \\n            // è®¾ç½®çŠ¶æ€