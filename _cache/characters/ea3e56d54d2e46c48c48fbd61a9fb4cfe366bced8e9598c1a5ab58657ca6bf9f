s[0].match(/^\\\\[(.*?)çš„æœ‹å‹åœˆå†…å®¹\\\\]$/);\\n            \\n            // å¦‚æœç¬¬ä¸€è¡ŒåŒ…å«\\\"æˆ‘\\\"ï¼Œè¿”å›é»˜è®¤ç”¨æˆ·ä¿¡æ¯\\n            if (sectionMatch && sectionMatch[1].includes('æˆ‘')) {\\n                return {\\n                    name: 'æˆ‘',\\n                    isCurrentUser: true\\n                };\\n            }\\n            \\n            // å¦åˆ™ä½¿ç”¨ç¬¬ä¸€æ¡åŠ¨æ€çš„ç”¨æˆ·ä¿¡æ¯\\n            const firstMomentMatch = lines[1].match(/^\\\\{(.*?)\\\\|(.*?)\\\\|/);\\n            if (firstMomentMatch) {\\n                return {\\n                    name: firstMomentMatch[1],\\n                    avatar: firstMomentMatch[2]\\n                };\\n            }\\n        }\\n\\n        // è§£ææœ‹å‹åœˆå†…å®¹\\n        function parseMomentsContent(content) {\\n            const moments = [];\\n            const lines = content.split('\\\\n');\\n            let currentMoment = null;\\n            let currentSection = null;\\n            let sectionAvatar = null;\\n            \\n            for (const line of lines) {\\n                // åŒ¹é…æœ‹å‹åœˆå†…å®¹åˆ†ç»„æ ‡è®°\\n                const sectionMatch = line.match(/^\\\\[(.*?)çš„æœ‹å‹åœˆå†…å®¹\\\\]$/);\\n                \\n                // åŒ¹é…æœ‹å‹åœˆä¸»å†…å®¹ï¼Œä¿®æ”¹æ­£åˆ™è¡¨è¾¾å¼ä»¥æ”¯æŒå¤šä¸ªå›¾ç‰‡æè¿°\\n                const momentMatch = line.match(/^\\\\{(.*?)\\\\|(.*?)\\\\|(.*?)(?:\\\\|å›¾ç‰‡ï¼š.*?)?\\\\|([0-9ï¼-ï¼™]+[:ï¼š][0-9ï¼-ï¼™]+)\\\\}$/);\\n                \\n                // åŒ¹é…è¯„è®ºå†…å®¹\\n                const commentMatch = line.match(/^\\\\{\\\\[(.*?)\\\\|(.*?)\\\\|(.*?)\\\\|([0-9ï¼-ï¼™]+[:ï¼š][0-9ï¼-ï¼™]+)\\\\]\\\\}$/);\\n                \\n                // åŒ¹é…é€šçŸ¥å†…å®¹\\n                const notificationMatch = line.match(/^\\\\[(.*?)\\\\|(.*?)\\\\|(.*?)\\\\|([0-9ï¼-ï¼™]+[:ï¼š][0-9ï¼-ï¼™]+)\\\\]$/);\\n                \\n                if (sectionMatch) {\\n                    // å¦‚æœæ˜¯åˆ†ç»„æ ‡è®°ï¼Œæ›´æ–°å½“å‰åˆ†ç»„\\n                    currentSection = sectionMatch[1];\\n                    \\n                    // æ ¹æ®åˆ†ç»„åç§°ç¡®å®šå¤´åƒ\\n                    if (currentSection === 'æˆ‘') {\\n                        sectionAvatar = 'ljn8c6.png';\\n                    } else if (currentSection === 'é¡¾é’åº') {\\n                        sectionAvatar = '7yxs1n.png';\\n                    } else if (currentSection === 'Mori') {\\n                        sectionAvatar = 'rt1xcj.png';\\n                    } else {\\n                        sectionAvatar = null;\\n                    }\\n                } else if (momentMatch) {\\n                    // å¦‚æœæ˜¯æ–°çš„æœ‹å‹åœˆå†…å®¹\\n                    const [_, name, avatar, text, time] = momentMatch;\\n                    \\n                    // æå–æ‰€æœ‰å›¾ç‰‡æè¿°\\n                    const imageDescs = line.match(/\\\\|å›¾ç‰‡ï¼š([^|]*?)(?=\\\\||$)/g);\\n                    const formattedImageDesc = imageDescs ? imageDescs.map(desc => desc.replace('|å›¾ç‰‡ï¼š', '')).join('|') : null;\\n                    \\n                    currentMoment = {\\n                        id: moments.length + 1,\\n                        user: {\\n                            name,\\n                            avatar\\n                        },\\n                        content: text,\\n                        time,\\n                        imageDesc: formattedImageDesc,\\n                        comments: [],\\n                        notifications: [],  // æ”¹ä¸ºæ•°ç»„å­˜å‚¨æ‰€æœ‰é€šçŸ¥\\n                        section: currentSection,\\n                        sectionAvatar: sectionAvatar\\n                    };\\n                    moments.push(currentMoment);\\n                } else if (commentMatch && currentMoment) {\\n                    // å¦‚æœæ˜¯è¯„è®º\\n                    const [_, name, avatar, text, time] = commentMatch;\\n                    currentMoment.comments.push({\\n                        id: currentMoment.comments.length + 1,\\n                        user: {\\n                            name,\\n                            avatar\\n                        },\\n                        content: text,\\n                        time,\\n                        notifications: []  // ä¸ºæ¯ä¸ªè¯„è®ºæ·»åŠ é€šçŸ¥æ•°ç»„\\n                    });\\n                } else if (notificationMatch && currentMoment) {\\n                    // å¦‚æœæ˜¯é€šçŸ¥\\n                    const [_, name, avatar, text, time] = notificationMatch;\\n                    const notification = {\\n                        name,\\n                        avatar,\\n                        content: text,\\n                        time\\n                    };\\n                    \\n                    // å°†é€šçŸ¥æ·»åŠ åˆ°å½“å‰æœ‹å‹åœˆçš„é€šçŸ¥åˆ—è¡¨ä¸­\\n                    currentMoment.notifications.push(notification);\\n                    \\n                    // å¦‚æœæœ‰è¯„è®ºï¼Œä¹Ÿå°†é€šçŸ¥å…³è”åˆ°æœ€åä¸€æ¡è¯„è®º\\n                    if (currentMoment.comments.length > 0) {\\n                        const lastComment = currentMoment.comments[currentMoment.comments.length - 1];\\n                        lastComment.notifications.push(notification);\\n                    }\\n                }\\n            }\\n            return moments;\\n        }\\n\\n        // å­˜å‚¨å½“å‰è¦å›å¤çš„ç›®æ ‡\\n        let currentReplyTarget = {\\n            momentId: null,\\n            commentId: null,\\n            type: null, // 'moment', 'comment'\\n            targetName: '',\\n            targetContent: '',\\n            targetTime: '',\\n            targetAvatar: ''\\n        };\\n\\n        // æ˜¾ç¤ºè¯„è®ºå¡ç‰‡\\n        function showCommentCard(momentId, commentId = null, targetName, commentData = null) {\\n            currentReplyTarget.momentId = momentId;\\n            currentReplyTarget.commentId = commentId;\\n            currentReplyTarget.type = commentId ? 'comment' : 'moment';\\n            currentReplyTarget.targetName = targetName;\\n            \\n            const commentCard = document.querySelector('.comment-card');\\n            const overlay = document.querySelector('.overlay');\\n            const commentCardTitle = document.querySelector('.comment-card-title');\\n            const commentCardInput = document.querySelector('.comment-card-input');\\n            \\n            commentCardTitle.textContent = commentId ? `å›å¤ ${targetName}` : 'å‘è¡¨è¯„è®º';\\n            commentCardInput.placeholder = commentId ? `å›å¤ ${targetName}...` : 'å†™è¯„è®º...';\\n            commentCardInput.value = '';\\n            commentCardInput.focus();\\n            \\n            commentCard.style.display = 'block';\\n            overlay.style.display = 'block';\\n            \\n            // å¦‚æœæœ‰è¯„è®ºæ•°æ®ï¼Œæ£€æŸ¥æ˜¯å¦æœ‰å¯¹åº”çš„é€šçŸ¥\\n            if (commentData) {\\n                const moments = parseMomentsContent(momentsContent);\\n                const moment = moments.find(m => m.id === momentId);\\n                \\n                if (moment) {\\n                    // æ„å»ºè¯„è®ºé”®\\n                    const commentKey = `${commentData.user.name}|${commentData.user.avatar}|${commentData.content}|${commentData.time}`;\\n                    \\n                    // æŸ¥æ‰¾åŒ¹é…çš„é€šçŸ¥\\n                    const notification = moment.notifications.find(n => n.name === commentData.user.name && n.avatar === commentData.user.avatar && n.content === commentData.content && n.time === commentData.time);\\n                    \\n                    if (notification) {\\n                        // æ˜¾ç¤ºé€šçŸ¥\\n                        setTimeout(() => {\\n                            showNotification(notification);\\n                        }, 600);\\n                    }\\n                }\\n            }\\n        }\\n\\n        // éšè—è¯„è®ºå¡ç‰‡\\n        function hideCommentCard() {\\n            const commentCard = document.querySelector('.comment-card');\\n            const overlay = document.querySelector('.overlay');\\n            \\n            commentCard.style.display = 'none';\\n            overlay.style.display = 'none';\\n            \\n            // é‡ç½®å½“å‰å›å¤ç›®æ ‡\\n            currentReplyTarget = {\\n                momentId: null,\\n                commentId: null,\\n                type: null,\\n                targetName: '',\\n                targetContent: '',\\n                targetTime: '',\\n                targetAvatar: ''\\n            };\\n        }\\n\\n        // é€šçŸ¥é˜Ÿåˆ—å’ŒçŠ¶æ€\\n        const notificationQueue = [];\\n        let isShowingNotification = false;\\n\\n        // æ˜¾ç¤ºé€šçŸ¥\\n        function showNotification(message) {\\n            // å°†é€šçŸ¥æ·»åŠ åˆ°é˜Ÿåˆ—\\n            notificationQueue.push(message);\\n            \\n            // å¦‚æœå½“å‰æ²¡æœ‰åœ¨æ˜¾ç¤ºé€šçŸ¥ï¼Œå¼€å§‹æ˜¾ç¤º\\n            if (!isShowingNotification) {\\n                showNextNotification();\\n            }\\n        }\\n\\n        // æ˜¾ç¤ºä¸‹ä¸€ä¸ªé€šçŸ¥\\n        function showNextNotification() {\\n            if (notificationQueue.length === 0) {\\n                isShowingNotification = false;\\n                return;\\n            }\\n\\n            isShowingNotification = true;\\n            const message = notificationQueue.shift();\\n            const notification = document.getElementById('notification');\\n            const dynamicIsland = document.querySelector('.dynamic-island');\\n            \\n            // æ’­æ”¾æç¤ºéŸ³\\n            const messageSound = document.getElementById('messageSound');\\n            messageSound.volume = CONFIG.BANNER_VOLUME;\\n            messageSound.currentTime = 0;\\n            messageSound.play().catch(error => {\\n                console.log('æ— æ³•æ’­æ”¾æç¤ºéŸ³:', error);\\n            });\\n            \\n            dynamicIsland.style.opacity = '0';\\n            \\n            notification.innerHTML = `\\n                <div class=\\\"notification-icon\\\">\\n                    <img src=\\\"https://files.catbox.moe/${message.avatar}\\\" alt=\\\"${message.name}\\\">\\n                </div>\\n                <div class=\\\"notification-content\\\">\\n                    <div class=\\\"notification-title\\\">${message.name}</div>\\n                    <div class=\\\"notification-message\\\">${message.content}</div>\\n                </div>\\n                <div class=\\\"notification-time\\\">${message.time}</div>\\n            `;\\n            \\n            notification.classList.add('show');\\n            notification.style.display = 'flex';\\n            \\n            setTimeout(() => {\\n                notification.classList.remove('show');\\n                setTimeout(() => {\\n                    dynamicIsland.style.opacity = '1';\\n                    notification.style.display = 'none';\\n                    // æ˜¾ç¤ºé˜Ÿåˆ—ä¸­çš„ä¸‹ä¸€ä¸ªé€šçŸ¥\\n                    showNextNotification();\\n                }, 300);\\n            }, CONFIG.NOTIFICATION_DURATION);\\n        }\\n\\n        // å¤„ç†è¯„è®º\\n        function handleComment(momentId, userName) {\\n            const moments = parseMomentsContent(momentsContent);\\n            const moment = moments.find(m => m.id === momentId);\\n            \\n            showCommentCard(momentId, null, userName);\\n            \\n            if (moment) {\\n                // æ‰¾åˆ°å½“å‰åŠ¨æ€åçš„æ‰€æœ‰é€šçŸ¥ï¼Œç›´åˆ°é‡åˆ°ä¸‹ä¸€ä¸ªåŠ¨æ€æˆ–è¯„è®º\\n                const lines = momentsContent.split('\\\\n');\\n                const notifications = [];\\n                let foundMoment = false;\\n                \\n                for (const line of lines) {\\n                    // åŒ¹é…å½“å‰åŠ¨æ€\\n                    const momentMatch = line.match(/^\\\\{(.*?)\\\\|(.*?)\\\\|(.*?)(?:\\\\|å›¾ç‰‡ï¼š.*?)?\\\\|([0-9ï¼-ï¼™]+[:ï¼š][0-9ï¼-ï¼™]+)\\\\}$/);\\n                    if (momentMatch) {\\n                        if (foundMoment) {\\n  