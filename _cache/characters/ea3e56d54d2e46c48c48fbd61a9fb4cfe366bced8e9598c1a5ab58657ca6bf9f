\n            if (titleMatch) {\\n                let [_, fullTitle] = titleMatch;\\n                let title = fullTitle;\\n                \\n                // å¦‚æœæœ‰æ‹¬å·ï¼Œä»fullTitleä¸­æå–åå­—ï¼ˆç¬¬ä¸€ä¸ªæ‹¬å·å‰çš„å†…å®¹ï¼‰\\n                if (remarksMatch) {\\n                    title = fullTitle.split('ï¼ˆ')[0].trim();\\n                }\\n                \\n                const chatTitle = document.querySelector('.chat-nav .contact');\\n                \\n                // å¤„ç†å¤‡æ³¨\\n                let remarksHtml = '';\\n                if (remarksMatch) {\\n                    const remarks = remarksMatch.map(r => r.replace(/[ï¼ˆï¼‰]/g, '')); // ç§»é™¤æ‹¬å·\\n                    remarksHtml = `\\n                        <div class=\\\"remarks-container\\\">\\n                            ${remarks.map(r => `<span class=\\\"contact-remark\\\" onclick=\\\"if(typeof triggerSlash === 'function') { triggerSlash('/send åœ¨é€šè®¯å½•ç•Œé¢æŸ¥çœ‹æ‰€æœ‰${r}æ ‡ç­¾çš„è”ç³»äºº|/trigger'); event.stopPropagation(); }\\\">${r}</span>`).join('')}\\n                        </div>\\n                    `;\\n                }\\n                \\n                chatTitle.innerHTML = `\\n                    <span class=\\\"main-title\\\">${title}</span>\\n                    ${remarksHtml}\\n                `;\\n                titleContacts = [title];\\n            }\\n            \\n            for (let i = 1; i < lines.length; i++) {\\n                const line = lines[i].trim();\\n                if (!line) continue;\\n                \\n                // æ£€æŸ¥æ˜¯å¦æ˜¯å¤´åƒæ›´æ¢çš„ç³»ç»Ÿæ¶ˆæ¯\\n                const systemMessageMatch = line.match(/\\\\[ç³»ç»Ÿæ¶ˆæ¯\\\\|(.+?)\\\\|(.+?)\\\\|([0-9]+:[0-9]+)\\\\]/);\\n                if (systemMessageMatch) {\\n                    const [_, title, content, time] = systemMessageMatch;\\n                    currentMessageIndex++;\\n                    \\n                    // æ£€æŸ¥æ˜¯å¦æ˜¯å¤´åƒæ›´æ¢æ¶ˆæ¯\\n                    const avatarChangeMatch = content.match(/ç”¨æˆ·å·²è¢«æ¢å¤´åƒï¼š(.+?)â€”æ¥è‡ª/);\\n                    if (avatarChangeMatch) {\\n                        tempNewAvatar = `https://files.catbox.moe/${avatarChangeMatch[1]}`;\\n                        avatarChangeIndex = currentMessageIndex;\\n                    }\\n                    \\n                    messages.push({\\n                        sender: title,\\n                        text: content,\\n                        time: time,\\n                        type: 'system',\\n                        messageIndex: currentMessageIndex\\n                    });\\n                    \\n                    // å¦‚æœæ˜¯å¤´åƒæ›´æ¢æ¶ˆæ¯ï¼Œæ·»åŠ ç”¨æˆ·æ¶ˆæ¯\\n                    if (avatarChangeMatch) {\\n                        currentMessageIndex++;\\n                        messages.push({\\n                            sender: 'æˆ‘',\\n                            text: 'ç”¨æˆ·å·²è¢«æ¢å¤´åƒ',\\n                            type: 'sent',\\n                            time: getCurrentTime(),\\n                            messageIndex: currentMessageIndex,\\n                            avatar: tempNewAvatar\\n                        });\\n                    }\\n                    \\n                    continue;\\n                }\\n                \\n                // å¤„ç†å¤§æ‹¬å·æ ¼å¼çš„æ¶ˆæ¯\\n                const bracketMatch = line.match(/\\\\{(.+?)(?:\\\\|([^}]+))?\\\\}/);\\n                if (bracketMatch) {\\n                    const [_, sender, content] = bracketMatch;\\n                    currentMessageIndex++;\\n                    \\n                    // æ£€æŸ¥æ˜¯å¦æ˜¯åˆ†äº«æ–°å¥½å‹æˆ–æ–°ç¾¤èŠçš„æ¶ˆæ¯\\n                    const newFriendMatch = content ? content.match(/åˆ†äº«ï¼šæ·»åŠ æ–°å¥½å‹ï¼š(.+?)(?:\\\\||$)/) : null;\\n                    const newGroupMatch = content ? content.match(/åˆ†äº«ï¼šåŠ å…¥æ–°ç¾¤èŠï¼š(.+?)(?:\\\\||$)/) : null;\\n                    const waimaiMatch = 