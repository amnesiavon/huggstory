Ÿæ˜¾éšåˆ‡æ¢ (ä¸å˜)\\n      importToggleButton.addEventListener('click', () => { isImportVisible = !isImportVisible; importContent.classList.toggle('show', isImportVisible); importToggleButton.textContent = isImportVisible ? 'æ”¶èµ·å¯¼å…¥' : 'å¯¼å…¥æ­Œæ›²'; });\\n\\n      // æ‰¹é‡/å•æ›²æ¨¡å¼åˆ‡æ¢ (ä¸å˜)\\n      batchModeCheckbox.addEventListener('change', updateImportUI);\\n\\n      // å¯¼å…¥æ­Œæ›²å¤„ç†é€»è¾‘ (ä¸å˜)\\n      importButton.addEventListener('click', () => { const isBatchMode = batchModeCheckbox.checked; const urlsInput = importUrlInput.value.trim(); const titleInput = importTitleInput.value.trim(); if (!urlsInput) { alert(\\\"è¯·è¾“å…¥æ­Œæ›²é“¾æ¥ï¼\\\"); return; } let importedCount = 0; let newTracks = []; if (isBatchMode) { const urls = urlsInput.split(/[,;\\\\n]+/).map(url => url.trim()).filter(url => url); if (urls.length === 0) { alert(\\\"æœªæ£€æµ‹åˆ°æœ‰æ•ˆçš„é“¾æ¥æ ¼å¼ã€‚\\\"); return; } urls.forEach(url => { if ((url.startsWith('http://') || url.startsWith('https://')) && !playlist.some(track => track.url === url)) { let title = \\\"æœªçŸ¥æ­Œæ›²\\\"; try { const urlParts = new URL(url); const pathnameParts = urlParts.pathname.split('/'); const filename = pathnameParts[pathnameParts.length - 1]; if (filename) { title = decodeURIComponent(filename.replace(/\\\\.[^/.]+$/, \\\"\\\")); } } catch (e) { title = url.substring(url.lastIndexOf('/') + 1, url.lastIndexOf('/') + 21) || \\\"æœªçŸ¥æ­Œæ›²\\\"; } newTracks.push({ url: url, title: title, source: \\\"å¯¼å…¥\\\" }); importedCount++; } else if (!url.startsWith('http://') && !url.startsWith('https://')) { console.warn(`æ— æ•ˆURL: ${url}`); } else { console.log(`æ­Œæ›²å·²å­˜åœ¨: ${url}`); } }); } else { const url = urlsInput; if ((url.startsWith('http://') || url.startsWith('https://')) && !playlist.some(track => track.url === url)) { let title = titleInput || \\\"æœªçŸ¥æ­Œæ›²\\\"; if (title === \\\"æœªçŸ¥æ­Œæ›²\\\") { try { const urlParts = new URL(url); const pathnameParts = urlParts.pathname.split('/'); const filename = pathnameParts[pathnameParts.length - 1]; if (filename) { title = decodeURIComponent(filename.replace(/\\\\.[^/.]+$/, \\\"\\\")); } } catch (e) { /* ä¿æŒ \\\"æœªçŸ¥æ­Œæ›²\\\" */ } } newTracks.push({ url: url, title: title, source: \\\"å¯¼å…¥\\\" }); importedCount++; } else if (!url.startsWith('http://') && !url.startsWith('https://')) { alert(`æ— æ•ˆçš„URLæ ¼å¼: ${url}`); return; } else { alert(`æ­Œæ›²é“¾æ¥å·²å­˜åœ¨: ${url}`); return; } } if (importedCount > 0) { playlist.push(...newTracks); savePlaylistToStorage(); renderPlaylist(); alert(`æˆåŠŸå¯¼å…¥ ${importedCount} é¦–æ­Œæ›²ï¼`); importUrlInput.value = ''; importTitleInput.value = ''; if (playlist.length === importedCount) { loadTrack(0); } } else { alert(\\\"æ²¡æœ‰æ–°çš„æ­Œæ›²è¢«å¯¼å…¥ã€‚\\\"); } });\\n\\n      // åˆ é™¤æ­Œæ›²å¤„ç† (ä¸å˜)\\n      function deleteTrack(indexToDelete) { if (indexToDelete < 0 || indexToDelete >= playlist.length) return; const deletedTrack = playlist[indexToDelete]; const wasCurrentTrackDeleted = (indexToDelete === currentTrackIndex); playlist.splice(indexToDelete, 1); savePlaylistToStorage(); renderPlaylist(); if (wasCurrentTrackDeleted) { if (isPlaying) { audioElement.pause(); isPlaying = false; stopVisualizer(); playIcon.textContent = 'â–¶'; } loadTrack(indexToDelete); } else { if (indexToDelete < currentTrackIndex) { currentTrackIndex--; } updateActivePlaylistItem(); } console.log(`å·²åˆ é™¤æ­Œæ›²: ${deletedTrack.title}`); }\\n\\n      // æ§åˆ¶æŒ‰é’®å’Œäº‹ä»¶ç›‘å¬å™¨ (ä¸å˜)\\n      playPauseButton.addEventListener('click', () => { if (playlist.length === 0) return; if (isPlaying) { audioElement.pause(); playIcon.textContent = 'â–¶'; isPlaying = false; stopVisualizer(); } else { audioElement.play().then(() => { playIcon.textContent = 'â¸'; isPlaying = true; startVisualizer(); }).catch(err => { console.error('æ’­æ”¾æŒ‰é’®å‡ºé”™:', err); alert('æ’­æ”¾å‡ºé”™'); playIcon.textContent = 'â–¶'; isPlaying = false; stopVisualizer(); }); } });\\n      prevButton.addEven