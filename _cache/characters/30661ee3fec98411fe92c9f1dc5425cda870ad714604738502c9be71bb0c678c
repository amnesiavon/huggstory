tButton = document.getElementById('next'); const progressBar = document.querySelector('.progress-bar'); const progress = document.querySelector('.progress'); const currentTimeDisplay = document.querySelector('.current-time'); const totalTimeDisplay = document.querySelector('.total-time'); const volumeBar = document.querySelector('.volume-bar'); const volumeLevel = document.querySelector('.volume-level'); const cursorFollower = document.querySelector('.cursor-follower'); const visualizerBars = document.querySelectorAll('.visualizer-bar'); const trackNameElement = document.getElementById('track-name'); const trackSourceElement = document.getElementById('track-source'); const playlistToggleButton = document.getElementById('playlist-toggle'); const playlistContainer = document.getElementById('playlist-container'); const playModeButton = document.getElementById('play-mode');\\n      const importToggleButton = document.getElementById('import-toggle-button'); const importContent = document.getElementById('import-content'); const batchModeCheckbox = document.getElementById('batch-mode-checkbox'); const importUrlInput = document.getElementById('import-url'); const importTitleInput = document.getElementById('import-title'); const importButton = document.getElementById('import-button');\\n\\n      const audioElement = new Audio(); audioElement.preload = \\\"metadata\\\";\\n      let playlist = []; let isPlaying = false; let currentTrackIndex = 0; let currentPlayMode = PLAY_MODES.LOOP; let isPlaylistVisible = false; let isImportVisible = false;\\n      const storageKey = 'gothamNexusPlayerPlaylist';\\n\\n      // --- æŒä¹…åŒ–å­˜å‚¨å‡½æ•° (ä¸å˜) ---\\n      function loadPlaylistFromStorage() { const storedPlaylist = localStorage.getItem(storageKey); if (storedPlaylist) { try { playlist = JSON.parse(storedPlaylist); if (!Array.isArray(playlist) || playlist.length === 0 || !playlist.every(item => item && typeof item.url === 'string' && typeof item.title === 'string')) { console.warn(\\\"localStorageæ’­æ”¾åˆ—è¡¨æ— æ•ˆæˆ–ä¸ºç©ºï¼Œä½¿ç”¨é»˜è®¤åˆ—è¡¨ã€‚\\\"); playlist = [...defaultPlaylist]; savePlaylistToStorage(); } } catch (e) { console.error(\\\"åŠ è½½æ’­æ”¾åˆ—è¡¨å¤±è´¥:\\\", e); playlist = [...defaultPlaylist]; savePlaylistToStorage(); } } else { playlist = [...defaultPlaylist]; savePlaylistToStorage(); } if (currentTrackIndex >= playlist.length) currentTrackIndex = 0; }\\n      function savePlaylistToStorage() { try { localStorage.setItem(storageKey, JSON.stringify(playlist)); } catch (e) { console.error(\\\"ä¿å­˜æ’­æ”¾åˆ—è¡¨å¤±è´¥:\\\", e); alert(\\\"æ— æ³•ä¿å­˜æ’­æ”¾åˆ—è¡¨ã€‚\\\"); } }\\n\\n      // åˆå§‹åŒ–æ’­æ”¾æ¨¡å¼æŒ‰é’®\\n      playModeButton.textContent = `æ’­æ”¾æ¨¡å¼: ${currentPlayMode}`;\\n\\n      // åˆå§‹åŒ–æ’­æ”¾å™¨\\n      function initPlayer() {\\n        loadPlaylistFromStorage();\\n        if (playlist.length > 0) { loadTrack(currentTrackIndex); }\\n        else { trackNameElement.textContent = \\\"æ’­æ”¾åˆ—è¡¨ä¸ºç©º\\\"; trackSourceElement.textContent = \\\"è¯·å¯¼å…¥æ­Œæ›²\\\"; currentTimeDisplay.textContent = '00:00'; totalTimeDisplay.textContent = '00:00'; }\\n        renderPlaylist();\\n        updateImportUI(); // åˆå§‹åŒ–å¯¼å…¥UI\\n        // ç¡®ä¿æ’­æ”¾åˆ—è¡¨å’Œå¯¼å…¥åŒºåŸŸåˆå§‹çŠ¶æ€æ­£ç¡®\\n        playlistContainer.classList.toggle('show', isPlaylistVisible);\\n        importContent.classList.toggle('show', isImportVisible);\\n        playlistToggleButton.textContent = isPlaylistVisible ? 'éšè—æ’­æ”¾åˆ—è¡¨' : 'æ˜¾ç¤ºæ’­æ”¾åˆ—è¡¨';\\n        importToggleButton.textContent = isImportVisible ? 'æ”¶èµ·å¯¼å…¥' : 'å¯¼å…¥æ­Œæ›²';\\n\\n        attemptAutoplay();\\n      }\\n\\n      // å°è¯•è‡ªåŠ¨æ’­æ”¾ (ä¸å˜)\\n      function attemptAutoplay() { if (playlist.length === 0 || !audioElement.src) return; const playPromise = audioElement.play(); if (playPromise !== undefined) { playPromise.then(() => { playIcon.textContent = 'â¸'; isPlaying = true; startVisualizer(); console.log(\\\"éŸ³é¢‘å·²è‡ªåŠ¨æ’­æ”¾ã€‚\\\"); }).catch(error => { console.warn('è‡ªåŠ¨æ’­æ”¾å¤±è´¥:', error.message); console.log(\\\"è‡ªåŠ¨æ’­æ”¾è¢«æµè§ˆå™¨é˜»æ­¢...\\\"); playIcon.textContent = 'â–¶'; isPlaying = false; stopVisualizer(); }); } }\\n\\n      // åŠ è½½æ›²ç›® (ä¸å˜)\\n      function loadTrack(index) { if (playlist.length === 0) { if (isPlaying) { audioElement.pause(); playIcon.textContent = 'â–¶'; isPlaying = false; stopVisualizer(); } trackNameElement.textContent = \\\"æ’­æ”¾åˆ—è¡¨ä¸ºç©º\\\"; trackSourceElement.textContent = \\\"è¯·å¯¼å…¥æ­Œæ›²\\\"; progress.style.width = '0%'; currentTimeDisplay.textContent = '00:00'; totalTimeDisplay.textContent = '00:00'; updateActivePlaylistItem(); return; } if (index < 0) index = playlist.length - 1; if (index >= playlist.length) index = 0; currentTrackIndex = index; const track = playlist[currentTrackIndex]; if (isPlaying) { audioElement.pause(); } audioElement.src = track.url; trackNameElement.textContent = track.title; trackSourceElement.textContent = track.source || \\\"æœªçŸ¥æ¥æº\\\"; progress.style.width = '0%'; currentTimeDisplay.textContent = '00:00'; totalTimeDisplay.textContent = '00:00'; audioElement.load(); updateActivePlaylistItem(); }\\n\\n      // æ¸²æŸ“æ’­æ”¾åˆ—è¡¨ (ä¸å˜)\\n      function renderPlaylist() { playlistContainer.innerHTML = ''; playlist.forEach((track, index) => { const item = document.createElement('div'); item.className = `playlist-item ${index === currentTrackIndex ? 'active' : ''}`; item.dataset.index = index; const infoContainer = document.createElement('div'); infoContainer.className = 'playlist-item-info'; const number = document.createElement('div'); number.className = 'playlist-item-number'; number.textContent = (index + 1).toString().padStart(2, '0'); const title = document.createElement('div'); title.className = 'playlist-item-title'; title.textContent = track.title; title.title = `${track.title} - ${track.source || 'æœªçŸ¥æ¥æº'}\\\\nURL: ${track.url}`; infoContainer.appendChild(number); infoContainer.appendChild(title); const de