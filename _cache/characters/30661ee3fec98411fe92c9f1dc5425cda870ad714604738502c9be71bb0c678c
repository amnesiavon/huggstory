 { width: 35px; height: 35px; }\\n        .play-pause { width: 45px; height: 45px; }\\n        .import-content input[type=\\\"text\\\"] { font-size: 12px; }\\n        .styled-button { font-size: 11px; padding: 4px 8px; }\\n        .import-content { padding: 10px; }\\n      }\\n    </style>\\n</head>\\n<body>\\n    <!-- disable-default-loading -->\\n    <div class=\\\"cursor-follower\\\"></div>\\n    <div class=\\\"player-container\\\">\\n      <div class=\\\"title-bar\\\">\\n        <div class=\\\"player-title\\\">GOTHAM NEXUS PLAYER</div>\\n        <div class=\\\"tech-dots\\\"><div class=\\\"tech-dot\\\"></div><div class=\\\"tech-dot\\\"></div><div class=\\\"tech-dot\\\"></div></div>\\n      </div>\\n\\n      <div class=\\\"track-info\\\">\\n        <div class=\\\"track-name\\\" id=\\\"track-name\\\">...</div> <div class=\\\"track-source\\\" id=\\\"track-source\\\">...</div>\\n      </div>\\n      <div class=\\\"visualizer\\\">\\n        <div class=\\\"visualizer-bar\\\" style=\\\"height: 40%;\\\"></div><div class=\\\"visualizer-bar\\\" style=\\\"height: 60%;\\\"></div><div class=\\\"visualizer-bar\\\" style=\\\"height: 30%;\\\"></div><div class=\\\"visualizer-bar\\\" style=\\\"height: 80%;\\\"></div><div class=\\\"visualizer-bar\\\" style=\\\"height: 50%;\\\"></div><div class=\\\"visualizer-bar\\\" style=\\\"height: 70%;\\\"></div><div class=\\\"visualizer-bar\\\" style=\\\"height: 35%;\\\"></div><div class=\\\"visualizer-bar\\\" style=\\\"height: 65%;\\\"></div><div class=\\\"visualizer-bar\\\" style=\\\"height: 45%;\\\"></div><div class=\\\"visualizer-bar\\\" style=\\\"height: 75%;\\\"></div><div class=\\\"visualizer-bar\\\" style=\\\"height: 25%;\\\"></div><div class=\\\"visualizer-bar\\\" style=\\\"height: 55%;\\\"></div><div class=\\\"visualizer-bar\\\" style=\\\"height: 85%;\\\"></div><div class=\\\"visualizer-bar\\\" style=\\\"height: 45%;\\\"></div><div class=\\\"visualizer-bar\\\" style=\\\"height: 60%;\\\"></div><div class=\\\"visualizer-bar\\\" style=\\