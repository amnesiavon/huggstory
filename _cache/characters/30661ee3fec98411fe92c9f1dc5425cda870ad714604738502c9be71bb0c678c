ment.createElement('div');\\n        earthDiv.className = 'dark-earth';\\n        earthDiv.style.left = `${x}px`;\\n        earthDiv.style.top = `${y}px`;\\n        earthDiv.style.background = `radial-gradient(circle at 30% 30%, ${earth.color}, #000)`;\\n        earthDiv.setAttribute('data-name', `${earth.name}: ${earth.desc}`);\\n        earthDiv.innerHTML = earth.name;\\n\\n        earthDiv.addEventListener('click', () => travelToEarth(earth.id, 'dark'));\\n        circle.appendChild(earthDiv);\\n    });\\n}\\n\\n// æ—…è¡Œåˆ°æŒ‡å®šåœ°çƒ\\nasync function travelToEarth(earthId, universeType) {\\n    const portalEffect = document.getElementById('portalEffect');\\n    portalEffect.style.opacity = '1';\\n\\n    try {\\n        let message;\\n        let earthData;\\n        let detailedDesc;\\n\\n        if (universeType === 'normal') {\\n            earthData = earths.find(earth => earth.id === earthId);\\n            detailedDesc = earthData ? earthData.detailedDesc : \\\"æœªçŸ¥ä¸–ç•Œ\\\";\\n            message = `{{user}}é€‰æ‹©è¿›å…¥DCå¤šå…ƒå®‡å®™çš„åœ°çƒ${earthId}ï¼ˆ${earthData.desc}ï¼‰ï¼Œè¿™æ˜¯ä¸€ä¸ª${detailedDesc}ã€‚åœ°çƒ${earthId}ä¸–ç•Œå¼€å§‹åŠ è½½ã€‚`;\\n        } else {\\n            earthData = darkEarths.find(earth => earth.id === earthId);\\n            detailedDesc = earthData ? earthData.detailedDesc : \\\"æœªçŸ¥çš„é»‘æš—ä¸–ç•Œ\\\";\\n            message = `{{user}}é€‰æ‹©è¿›å…¥DCé»‘æš—å¤šå…ƒå®‡å®™çš„åœ°çƒ${earthId}ï¼ˆ${earthData.desc}ï¼‰ï¼Œè¿™æ˜¯ä¸€ä¸ª${detailedDesc}ã€‚åœ°çƒ${earthId}é»‘æš—ä¸–ç•Œå¼€å§‹åŠ è½½ã€‚`;\\n        }\\n\\n        // ä½¿ç”¨triggerSlashå‡½æ•°å‘é€æ¶ˆæ¯\\n        await triggerSlash(`/send ${message}`);\\n        await triggerSlash('/trigger');\\n\\n        setTimeout(() => {\\n            portalEffect.style.opacity = '0';\\n        }, 800);\\n    } catch (error) {\\n        console.error('Error during travel:', error);\\n    }\\n}\\n\\n// åˆ‡æ¢å®‡å®™\\nfunction switchUniverse(toUniverse) {\\n    const universeSwitch = document.getElem