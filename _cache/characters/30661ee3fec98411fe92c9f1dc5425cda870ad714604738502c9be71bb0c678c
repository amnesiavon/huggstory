 function toggleDetail(sectionId, categoryElement) { playClickSound(); const section = document.getElementById(sectionId); if (!section) return; const isOpening = section.style.display !== 'block'; if (currentDetailSection) { currentDetailSection.style.display = 'none'; } if (currentCategoryElement) { currentCategoryElement.classList.remove('active'); } if (currentDetailSection === section && !isOpening) { currentDetailSection = null; currentCategoryElement = null; } else if (isOpening) { section.style.display = 'block'; categoryElement.classList.add('active'); currentDetailSection = section; currentCategoryElement = categoryElement; section.scrollIntoView({ behavior: 'smooth', block: 'nearest' }); } else { currentDetailSection = null; currentCategoryElement = null; } }\\n\\n        function loadTasksForSection(sectionId) {\\n            const section = document.getElementById(sectionId);\\n            const taskList = taskLibrary[sectionId];\\n            if (!section || !taskList) return;\\n\\n            section.innerHTML = `<h3>${section.querySelector('h3').innerText}</h3>`;\\n\\n            if (taskList.length === 0) {\\n                 section.innerHTML += `<div class=\\\"placeholder\\\">ä»»åŠ¡åº“ä¸ºç©ºã€‚</div>`;\\n                 return;\\n            }\\n\\n            const shuffledTasks = shuffleArray([...taskList]);\\n            const tasksToShow = shuffledTasks.slice(0, TASKS_TO_SHOW);\\n\\n            tasksToShow.forEach(task => {\\n                const item = document.createElement('div');\\n                item.classList.add('detail-item');\\n\\n                let fullTaskText = `${task.title}\\\\n`;\\n                if (task.description) fullTaskText += `${task.description}\\\\n`;\\n                if (task.reward) fullTaskText += `æŠ¥é…¬ï¼š${task.reward}\\\\n`;\\n                if (task.price) fullTaskText += `ä»·æ ¼ï¼š${task.price}\\\\n`;\\n                if (task.rank) fullTaskText += `${task.rank}\\\\n`;\\n                if (task.reliability) fullTaskText += `å¯é åº¦ï¼š${task.reliability}\\\\n`;\\n                fullTaskText = fullTaskText.trim();\\n\\n                item.onclick = () => sendAndTrigger(task.prefix, fullTaskText);\\n\\n                let contentHTML = `<strong>${task.title}</strong>`;\\n                if (task.description) contentHTML += `<span>${task.description}</span>`;\\n                if (task.reward) contentHTML += `<span class=\\\"reward\\\">æŠ¥é…¬ï¼š${task.reward}</span>`;\\n                if (task.price) contentHTML += `<span class=\\\"price\\\">ä»·æ ¼ï¼š${task.price}</span>`;\\n                if (task.rank) contentHTML += `<span class=\\\"rank\\\">${task.rank}</span>`;\\n                if (task.reliability) contentHTML += `<span class=\\\"reliability\\\">å¯é åº¦ï¼š${task.reliability}</span>`;\\n\\n                item.innerHTML = contentHTML;\\n                section.appendChild(item);\\n            });\\n        }\\n\\n        function refreshInterface() { playClickSound(); console.log(\\\"Refreshing tasks...\\\"); if (currentDetailSection) { currentDetailSection.style.display = 'none'; currentDetailSection = null; } if (currentCategoryElement) { currentCategoryElement.classList.remove('active'); currentCategoryElement = null; } Object.keys(taskLibrary).forEach(sectionId => { loadTasksForSection(sectionId); }); window.scrollTo({ top: 0, behavior: 'smooth' }); console.log(\\\"Tasks refreshed.\\\"); }\\n\\n        async function sendAndTrigger(prefix, fullTaskText) {\\n            playClickSound();\\n            const textToSend = `{{user}}${prefix}\\\\n${fullTaskText.replace(/\\\\n/g, '\\\\\\\\n')}`;\\n            const sendCommand = `/send ${textToSend}`;\\n            const triggerCommand = '/trigger';\\n\\n            if (typeof triggerSlash === 'function') {\\n                try {\\n                    console.log(`Executing: ${sendCommand}`);\\n                    await triggerSlash(sendCommand);\\n                    console.log(`Executing: ${triggerCommand}`);\\n                    await triggerSlash(triggerCommand);\\n                } catch (error) {\\n                    console.error(\\\"Error executing triggerSlash sequence:\\\", error);\\n                    alert(\\\"æ‰§è¡Œå‘½ä»¤æ—¶å‡ºé”™ã€‚\\\");\\n                }\\n            } else {\\n                console.warn(\\\"triggerSlash function not found. Cannot execute commands.\\\");\\n                alert(`æ¨¡æ‹Ÿæ‰§è¡Œ: /send {{user}}${prefix}\\\\\\\\n${fullTaskText.replace(/\\\\n/g, '\\\\\\\\n')}\\\\nç„¶å: /trigger\\\\n(å®é™…ç¯å¢ƒéœ€è¦å‰ç«¯åŠ©æ‰‹æ”¯æŒ)`);\\n            }\\n        }\\n\\n        document.addEventListener('DOMContentLoaded', () => { console.log(\\\"DOM loaded. Initializing task display...\\\"); Object.keys(taskLibrary).forEach(sectionId => { loadTasksForSection(sectionId); const section = document.getElementById(sectionId); if(section) section.style.display = 'none'; }); console.log(\\\"Initial tasks loaded.\\\"); });\\n    </script>\\n</body>\\n</html>\\n```\",\"trimStrings\":[],\"placement\":[2],\"disabled\":false,\"markdownOnly\":true,\"promptOnly\":false,\"runOnEdit\":true,\"substituteRegex\":0,\"minDepth\":null,\"maxDepth\":null},{\"id\":\"8ede7eda-afc0-4bfd-9474-e45ad438e452\",\"scriptName\":\"å“¥è°­åœ°å›¾\",\"findRegex\":\"\\\\[å“¥è°­åœ°å›¾\\\\]\",\"replaceString\":\"\\n```html\\n<html>\\n<head>\\n  <style>\\n    /* --- åŸºæœ¬æ ·å¼ --- */\\n    body {\\n      margin: 0;\\n      padding: 0;\\n      font-family: 'Arial', sans-serif; /* ä¿æŒå­—ä½“ä¸€è‡´ */\\n    }\\n    .gotham-map-container {\\n      position: relative;\\n      width: 100%;\\n      max-width: 1200px;\\n      margin: 0 auto;\\n      font-family: 'Arial', sans-serif;\\n      overflow: hidden;\\n      background-color: #0a0e17;\\n      border-radius: 8px;\\n      box-shadow: 0 0 20px rgba(0, 100, 255, 0.5);\\n      padding: 20px;\\n      color: #e0e0e0;\\n      box-sizing: border-box; /* ç¡®ä¿paddingä¸å½±å“æ€»å®½åº¦ */\\n    }\\n\\n    /* --- å¤´éƒ¨æ ·å¼ --- */\\n    .