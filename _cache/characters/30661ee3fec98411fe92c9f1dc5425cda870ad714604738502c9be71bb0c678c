');\\n      locationItems.forEach(item => {\\n        const locationName = item.dataset.location;\\n        if (locationName) {\\n          item.addEventListener('click', async () => {\\n            console.log(`Clicked on: ${locationName}`);\\n\\n            // *** 新增：播放点击音效 ***\\n            if (clickSound) {\\n              clickSound.currentTime = 0; // 从头开始播放\\n              clickSound.play().catch(error => {\\n                // 捕获并处理可能的播放错误 (例如，用户未与页面交互前不允许自动播放)\\n                console.error(\\\"Audio play failed:\\\", error);\\n              });\\n            }\\n\\n            const summary = `前往 ${locationName}`;\\n            try {\\n              // 依次执行 /send 和 /trigger 命令\\n              await triggerSlash(`/send ${summary}`);\\n              await triggerSlash('/trigger');\\n              console.log(`Triggered actions for: ${locationName}`);\\n            } catch (error) {\\n              console.error(\\\"Error triggering slash commands:\\\", error);\\n            }\\n          });\\n        } else {\\n            console.warn(\\\"Could not determine location name for item:\\\", item);\\n        }\\n      });\\n    }\\n\\n    // --- 初始化脚本 ---\\n    function initializeMapInterface() {\\n        updateRealTime();\\n        createRandomDots(15);\\n        moveRedAlertDot();\\n        addLocationClickListeners();\\n    }\\n\\n    if (document.readyState === 'loading') {\\n        document.addEventListener('DOMContentLoaded', initializeMapInterface);\\n    } else {\\n        initializeMapInterface();\\n    }\\n\\n  </script>\\n</body>\\n</html>\\n```\",\"trimStrings\":[],\"placement\":[2],\"disabled\":false,\"markdownOnly\":true,\"promptOnly\":false,\"runOnEdit\":true,\"substituteRegex\":0,\"minDepth\":null,\"maxDepth\":null},{\"id\":\"0db11b9e-bc10-48af-a2dc-e5c041467741\",\"scriptName\":\"哥谭黑市\",\"findRegex\":\"\\\\[哥谭黑市\\\\]\",\"replaceString\":\"```\\n<html>\\n<head>\\n    <title>哥谭黑市</title>\\n    <meta name=\\\"viewport\\\" content=\\\"width=devi