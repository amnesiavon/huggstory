ent: '×';\\n       position: absolute;\\n       top: 50%;\\n       left: 50%;\\n       transform: translate(-50%, -50%);\\n       color: white;\\n       font-size: 16px;\\n       font-weight: bold;\\n   }\\n   .step.current .step-dot {\\n       background: #1a1a24;\\n       border-color: #ffb56b;\\n       box-shadow: 0 0 0 4px rgba(26, 26, 36, 0.5), 0 0 15px rgba(255, 181, 107, 0.5);\\n       animation: pulse 1.5s infinite;\\n   }\\n   .step.current .step-label {\\n       color: #ffb56b;\\n       font-weight: 600;\\n       animation: glow 1.5s infinite alternate;\\n   }\\n   .step-detail {\\n       position: absolute;\\n       top: 100%;\\n       left: 50%;\\n       transform: translateX(-50%);\\n       background: rgba(39, 31, 51, 0.95);\\n       border: 1px solid rgba(168, 141, 255, 0.5);\\n       border-radius: 6px;\\n       padding: 10px;\\n       width: 200px;\\n       z-index: 10;\\n       color: #e0dbff;\\n       box-shadow: 0 4px 15px rgba(0, 0, 0, 0.3);\\n       font-size: 0.85em;\\n       line-height: 1.4;\\n       margin-top: 10px;\\n       text-align: left;\\n       white-space: normal;\\n   }\\n   .step-detail:before {\\n       content: '';\\n       position: absolute;\\n       top: -6px;\\n       left: 50%;\\n       transform: translateX(-50%);\\n       width: 0;\\n       height: 0;\\n       border-left: 6px solid transparent;\\n       border-right: 6px solid transparent;\\n       border-bottom: 6px solid rgba(168, 141, 255, 0.5);\\n   }\\n   @keyframes glow {\\n       from {\\n           text-shadow: 0 0 2px rgba(255, 181, 107, 0.3);\\n       }\\n       to {\\n           text-shadow: 0 0 10px rgba(255, 181, 107, 0.7);\\n       }\\n   }\\n   @keyframes pulse {\\n       0% {\\n           box-shadow: 0 0 0 4px rgba(26, 26, 36, 0.5), 0 0 5px rgba(255, 181, 107, 0.7);\\n       }\\n       70% {\\n           box-shadow: 0 0 0 4px rgba(26, 26, 36, 0.5), 0 0 15px rgba(255, 181, 107, 0.3);\\n       }\\n       100% {\\n           box-shadow: 0 0 0 4px rgba(26, 2