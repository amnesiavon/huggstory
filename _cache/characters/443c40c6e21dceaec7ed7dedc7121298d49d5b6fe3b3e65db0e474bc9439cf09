\\\"/g, (match, p1) => {\\n       return `\\\"<strong>${p1}</strong>\\\"`;\\n   });\\n   \\n   for (const [placeholder, original] of Object.entries(placeholders)) {\\n       text = text.replace(placeholder, original);\\n   }\\n   \\n   return text;\\n}\\nfunction parseGameText() {\\n   const rawContent = document.getElementById('gametext').innerHTML;\\n   \\n   const sections = {\\n       mainplot: {},\\n       statuspage: {},\\n       logpage: {}\\n   };\\n   \\n   for (const section in sections) {\\n       const regex = new RegExp(`<${section}>([\\\\\\\\s\\\\\\\\S]*?)</${section}>`);\\n       const match = rawContent.match(regex);\\n       if (match) {\\n           const lines = match[1].trim().split('\\\\n');\\n           sections[section] = {};\\n           lines.forEach(line => {\\n               if (line.startsWith('relationship|')) {\\n                   const relationshipData = line.substring('relationship|'.length);\\n                   \\n                   if (!sections[section]['relationship']) {\\n                       sections[section]['relationship'] = [];\\n                   }\\n                   sections[section]['relationship'].push(relationshipData);\\n               } else {\\n                   const [key, ...values] = line.split('|');\\n                   if (key === '正文') {\\n                       if (!sections[section][key]) {\\n                           sections[section][key] = [];\\n                       }\\n                       const processedText = wrapQuotedContent(values[0]);\\n                       sections[section][key].push(marked.parse(processedText));\\n                   } \\n                   else if (key === 'task') {\\n                       if (!sections[section][key]) {\\n                           sections[section][key] = [];\\n                       }\\n                       \\n                       const taskName = values[0] || '';\\n                       const taskDesc = values[1] || '';\\n                       const taskNodes = values.slice(2) || [];\\n                       \\n                       const parsedNodes = taskNodes.map(node => {\\n                           return node;\\n                       });\\n                       \\n                       sections[section][key].push({\\n                           name: taskName,\\n                           description: taskDesc,\\n                           nodes: parsedNodes\\n                       });\\n                   }\\n                   else if (key === 'Choices') {\\n                       sections[section][key] = values;\\n                   } else {\\n                       sections[section][key] = values;\\n                   }\\n               }\\n           });\\n       }\\n   }\\n   return sections;\\n}\\nfunction updateUI(sections) {\\n   updateMainText(sections.mainplot);\\n   updateStatusPage(sections.statuspage);\\n   updateLogPage(sections.logpage);\\n}\\nfunction updateMainText(mainplot) {\\n   if (!mainplot || !mainplot['正文']) return;\\n   \\n   const storyText = mainplot['正文'].join('');\\n   document.getElementById('main-text').innerHTML = `\\n       <div class=\\\"card fade-in\\\">\\n           <div class=\\\"card-header\\\">\\n               <h2 class=\\\"card-title\\\"><i class=\\\"fas fa-clock\\\"></i> ${mainplot['time'] ? mainplot['time'].join(' ') : ''}</h2>\\n           </div>\\n           <div style=\\\"display: flex; justify-content: space-between; margin-bottom: 15px;\\\">\\n               <p><i class=\\\"fas fa-map-marker-alt\\\"></i>  ${mainplot['location'] ? mainplot['location'][0] : ''}</p>\\n               <p><i class=\\\"fas fa-cloud-sun\\\"></i>  ${mainplot['weather'] ? mainplot['weather'][0] : ''}</p>\\n           </div>\\n           <div class=\\\"story-text\\\">${storyText}</div>\\n       </div>\\n   `;\\n}\\nfunction updateStatusPage(statuspage) {\\n   if (!statuspage) return;\\n   \\n   let statusHtml = '<div class=\\\"card fade-in\\\">';\\n   \\n   const baseStatus = [\\n       { key: '衣着', icon: 'fa-shirt' },\\n       { key: '姿势', icon: 'fa-person' },\\n       { key: '心声', icon: 'fa-comment-dots' }\\n   ];\\n   \\n   baseStatus.forEach(section => {\\n       if (statuspage[section.key] && statuspage[section.key][0]) {\\n           statusHtml += createStatusSection(section.key, statuspage[section.key][0], section.icon);\\n       }\\n   });\\n   \\n   if (statuspage['身体']) {\\n       statusHtml += `\\n           <div class=\\\"status-section\\\">\\n               <h3 class=\\\"status-title\\\"><i class=\\\"fas fa-user\\\"></i> 身体</h3>\\n               <div class=\\\"status-content\\\">${statuspage['身体'][0]}</div>\\n       `;\\n       \\n       statusHtml += `<div class=\\\"body-subsections\\\">`;\\n       \\n       if (statuspage['腋下']) {\\n           statusHtml += createFullSubSection('腋下', statuspage['腋下'][0], 'fa-grip-lines');\\n       }\\n       \\n       if (statuspage['胸部']) {\\n           let chestHtml = `\\n               <div class=\\\"subsection\\\">\\n                   <div class=\\\"subsection-header\\\" onclick=\\\"toggleCollapse(this)\\\">\\n                       <h4 class=\\\"subsection-title\\\"><i class=\\\"fas fa-heart\\\"></i> 胸部 <i class=\\\"fas fa-chevron-down toggle-icon\\\"></i></h4>\\n                   </div>\\n                   <div class=\\\"subsection-content\\\" style=\\\"display: block\\\">\\n                       <div class=\\\"status-content\\\">${statuspage['胸部'][0]}</div>\\n           `;\\n           \\n           if (statuspage['乳汁']) {\\n               chestHtml += `<div class=\\\"fluid-container\\\">`;\\n               const value = statuspage['乳汁'][0];\\n               const calculation = statuspage['乳汁'].length > 1 ? statuspage['乳汁'][1] : '';\\n               chestHtml += createFluidBarInSubSection('乳汁', value, calculation, 'milk-fill', 'fa-droplet');\\n               chestHtml += `</div>`;\\n           }\\n           \\n           chestHtml += `</div></div>`;\\n           statusHt